<div class="flex-c sscbox" [hidden]="!loadpage">
    <div class="flex-c ssccontain">
        <div class="contaner">
            <app-credtop [type]="'ssc'" [indata]="cpnav"></app-credtop>
            <div class="flex-r mainbox">
                <div class="flex-c contentbox">
                    <div class="flex-r tabbox">
                        <div class="flex-r tabcontent">
                            <span *ngFor="let item of typedata" [ngClass]="{tabactive:this.type == item.id}" (click)="togtype(item.id)">{{item.name}}</span>
                        </div>
                        <div class="flex-r togglebtn">
                            <span (click)="linkrouter('/lottery/officialssc')">官方</span>
                            <span class="togactive">信用</span>
                        </div>
                    </div>
                    <div class="flex-r selctbox" onselectstart="return false;" style="-moz-user-select:none;">
                        <span class="flex-r">
                            <strong>$</strong>
                            <span class="flex-r rangebox">
                                <span class="showvalue">{{(odds-rangevalue)| number : '1.0-1'}}%</span>
                                <span class="icon" (click)="rangevaluelessen()">-</span>
                                <span class="flex-r" style="width:130px; padding: 0 10px; align-items: center;">
                                    <input type="range" name="points" min="0" max="{{odds}}" step="0.1" id="points" [(ngModel)]="rangevalue">
                                </span>
                                <span class="icon" (click)="rangevalueadd()">+</span>
                            </span>
                        </span>
                        <span class="flex-r btngroup">
                            <button [ngClass]="{btnactive:selectbtnvalue === 0}" (click)="selectbtnvalue = 0">一般</button>
                            <button [ngClass]="{btnactive:selectbtnvalue === 1}" (click)="selectbtnvalue = 1">快捷</button>
                            <button [ngClass]="{btnactive:selectbtnvalue === 2}" (click)="selectbtnvalue = 2">快选金额</button>
                        </span>
                    </div>
                    <div class="flex-c mycontaion">
                        <!-- // 全5中1 -->
                        <div class="flex-c content" *ngIf="type === 3">
                            <div class="flex-r contenttop">
                                <span class="flex-r itembox" *ngFor="let item of contenttoptitle3">
                                    <span class="item1">号码</span>
                                    <span class="item2">赔率</span>
                                    <span class="item3" [hidden]="selectbtnvalue === 1">金额</span>
                                </span>

                            </div>
                            <div class="flex-r conmain">
                                <span class="flex-r itembox" *ngFor="let item of betdata3;let i=index" [ngStyle]="{'order': item.order}">
                                    <span class="item1">
                                        <span>{{item.numb}}</span>
                                    </span>
                                    <span class="item2 color_y">{{(2.099+(0.191/7.8*rangevalue))| number : '1.0-3'}} </span>
                                    <span class="item3" [hidden]="selectbtnvalue === 1">
                                        <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value" (focus)="inmoneyfocus($event,i)"
                                            (blur)="inmoneyblur()" (keyup)="changereg()">
                                    </span>
                                </span>
                            </div>
                        </div>
                        <!-- // 龙虎斗 -->
                        <div class="flex-c content" *ngIf="type === 2">
                            <div class="flex-r conmain2">
                                <div class="itemwrap2" *ngFor="let item of betdata2;let i=index">
                                    <div class="flex-c itembox2">
                                        <span class="item1">
                                            {{item.title}}
                                        </span>
                                        <span class="flex-r item2">
                                            <span class="flex-r item2info" [ngClass]="{togstyle2:selectbtnvalue == 1}">
                                                <span class="color_r">龙</span>
                                                <span class="color_y">{{(1.944+(0.224/7.8*rangevalue))| number : '1.0-3'}}</span>
                                            </span>
                                            <span class="flex-r item2info" [ngClass]="{togstyle2:selectbtnvalue == 1}">
                                                <span class="color_g">和</span>
                                                <span class="color_y">{{(8.98+(0.78/7.8*rangevalue))| number : '1.0-2'}}</span>
                                            </span>
                                            <span class="flex-r item2info" [ngClass]="{togstyle2:selectbtnvalue == 1}">
                                                <span class="color_b">虎</span>
                                                <span class="color_y">{{(1.944+(0.224/7.8*rangevalue))| number : '1.0-3'}}</span>
                                            </span>
                                        </span>
                                        <span class="flex-r item3" *ngIf="selectbtnvalue !== 1">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value1.value" (focus)="inmoney2focus($event,i,'value1')"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value2.value" (focus)="inmoney2focus($event,i,'value2')"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value3.value" (focus)="inmoney2focus($event,i,'value3')"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- 整合 -->
                        <div class="flex-c content content1" *ngIf="type === 1">
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox ballbox" *ngFor="let item of betdatam;let i=index">
                                    <div class="flex-r contenttop contenttop1">

                                        <span class="flex-r itembox">
                                            {{item.title}}
                                        </span>
                                    </div>
                                    <div class="flex-r contenttop contenttop3">
                                        <span class="flex-r itembox">
                                            <span class="item1">号码</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" [hidden]="selectbtnvalue === 1">金额</span>
                                        </span>

                                    </div>
                                            <span class="flex-r itembox" *ngFor="let child of item.data1; let q=index">
                                                <span class="item1">
                                                    {{child.name}}
                                                </span>
                                                <span class="item2 color_y">{{(1.8+(0.156/7.8*rangevalue))| number : '1.0-3'}} </span>
                                                <span class="item3" [hidden]="selectbtnvalue === 1">
                                                    <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="child.value" (focus)="inmoney1focus($event,i,'data1',q)"
                                                        (blur)="inmoneyblur()" (keyup)="changereg()">
                                                </span>
                                            </span>
                                    <span class="flex-r itembox" *ngFor="let child of item.data2; let q=index">
                                        <span class=" item1">
                                            <i>{{child.numb}}</i>
                                        </span>
                                        <span class="item2 color_y">{{(9+(0.78/7.8*rangevalue))| number : '1.0-3'}} </span>
                                        <span class=" item3" [hidden]="selectbtnvalue === 1">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="child.value" (focus)="inmoney1focus($event,i,'data2',q)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </span>
                                </div>
                                <!-- 空白部分 -->
                                <!-- <div class="flex-c conmainbox ballbox">
                                    <span class="flex-r itembox" *ngFor="let child of betdata1.data2[0];let q=index">
                                        <span class=" item1">
                                        </span>
                                        <span class="item2 color_y"></span>
                                        <span class=" item3" [hidden]="selectbtnvalue === 1">
                                        </span>
                                    </span>
                                </div> -->
                            </div>
                            <div class="flex-r contenttop contenttop2">
                                <span class="flex-r itembox" [ngClass]="{bg_y:btolast===0}" (click)="btolast=0">前三</span>
                                <span class="flex-r itembox" [ngClass]="{bg_y:btolast===1}" (click)="btolast=1">中三</span>
                                <span class="flex-r itembox" [ngClass]="{bg_y:btolast===2}" (click)="btolast=2">后三</span>

                            </div>
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox bomainbox" *ngFor="let item of betdatab; let i=index">

                                    <div class="flex-r contenttop contenttop4">
                                            <span class="flex-r itembox">
                                                <span class="item1">号码</span>
                                                <span class="item2">赔率</span>
                                                <span class="item3" [hidden]="selectbtnvalue === 1">金额</span>
                                            </span>
            
                                        </div>
                                    <span class="flex-r itembox">
                                        <span class="item1 itembold">
                                            {{item.name}}
                                        </span>
                                        <span class="item2 color_y">{{(11.633+(1.3/7.8*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" [hidden]="selectbtnvalue === 1">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value" (focus)="inmoney1focus($event, i,null,null)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </span>
                                </div>
                            </div>
                        </div>

                        <!-- 按钮 -->
                        <div class="flex-r subbox">
                            <span class="flex-r btngroup">
                                <span [hidden]="selectbtnvalue !== 1">
                                    <input type="text" maxlength="9" class="inmoney" id="allinmoney" name="allinmoney" [(ngModel)]="setallmoney.value" value=""
                                        (focus)="inmoneyfocus($event,'all')" (blur)="inmoneyblur()" (change)="allchange()" (keyup)="changereg()">
                                </span>
                                <button class="btnactive" [ngClass]="{lactive:selectbtnvalue !== 1 }" (click)="sub()">确定</button>
                                <button class="resetbtn" (click)="reset()">取消</button>
                            </span>

                        </div>
                        <!-- 活动选择框 -->
                        <div id="selectbox" [ngStyle]="{'top': boxposition.y, 'left': boxposition.x}" [hidden]="!boxshow">
                            <div class="flex-c selectwrap">
                                <i></i>
                                <span class="option" *ngFor="let item of selmoeny" (mousedown)="optinclick(item)">{{item}}元</span>
                            </div>
                        </div>
                        <!-- 底部问路-->
                        <div class="flex-c sscbottom" *ngIf="type!==3">
                            <ul class="flex-r tabbox">
                                <li class="tabitem">
                                    <select id="typeoption" name="typeoption" [(ngModel)]="typeoption" (change)="typeoptchange()">
                                        <option *ngFor="let item of typeoptiondata" value="{{item}}">{{item}}</option>
                                    </select>

                                </li>
                                <li class="tabitem" *ngFor="let item of bottomtabdata; let i=index">
                                    <span class="tabbtn" [ngClass]="{tabbtnactive:bottabactive == i }" (click)="bottabactive = i">
                                        {{item.name}}
                                    </span>
                                </li>
                            </ul>
                            <div class="flex-r wenlu">
                                <div class="flex-r wenlubtn">
                                    <strong></strong>
                                </div>
                                <!-- //大路 -->
                                <div class="flex-r wtablebox">
                                    <div class="flex-r wtable">
                                        <div class="iteminfo">
                                            {{tablebox1.name}}
                                        </div>
                                        <div class="wrapbox" [ngStyle]="{'width': tablebox1.width}">
                                            <div class="tablebox" id="tablebox1" [ngStyle]="{'left': tablebox1.left}" (mousedown)="dragdown($event,'tablebox1')" (mouseup)="tablebox1.drag = false"
                                                (mousemove)="dragmove($event,'tablebox1')" (mouseleave)="tablebox1.drag = false">
                                                <table id="table1" width="100%" border="0" cellspacing="0" cellpadding="4" bgcolor="#ccc" align="center">
                                                    <tr *ngFor="let item of tablebox1.data">
                                                        <td *ngFor="let child of item"></td>
                                                    </tr>

                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                            </div>
                            <!-- ===盘珠路======== -->
                            <div class="flex-r wenlu">
                                <div class="flex-r wenlubtn">
                                    <strong></strong>
                                </div>
                                <div class="flex-r wtablebox">
                                    <div class="flex-r wtable">
                                        <div class="iteminfo infosmall">
                                            {{tablebox2.name}}
                                        </div>
                                        <div class="wrapbox" [ngStyle]="{'width': tablebox2.width}">
                                            <div class="tablebox" id="tablebox2" [ngStyle]="{'left': tablebox2.left}" (mousedown)="dragdown($event,'tablebox2')" (mouseup)="tablebox2.drag = false"
                                                (mousemove)="dragmove($event,'tablebox2')" (mouseleave)="tablebox2.drag = false">
                                                <table id="table2" width="100%" border="0" cellspacing="0" cellpadding="4" bgcolor="#ccc" align="center">
                                                    <tr *ngFor="let item of tablebox2.data">
                                                        <td *ngFor="let child of item"></td>
                                                    </tr>

                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                    <!-- 大眼路 -->
                                    <div class="flex-r wtable">
                                        <div class="iteminfo infosmall">
                                            {{tablebox3.name}}
                                        </div>
                                        <div class="wrapbox" [ngStyle]="{'width': tablebox3.width}">
                                            <div class="tablebox" id="tablebox3" [ngStyle]="{'left': tablebox3.left}" (mousedown)="dragdown($event,'tablebox3')" (mouseup)="tablebox3.drag = false"
                                                (mousemove)="dragmove($event,'tablebox3')" (mouseleave)="tablebox3.drag = false">
                                                <table id="table3" width="100%" border="0" cellspacing="0" cellpadding="4" bgcolor="#ccc" align="center">
                                                    <tr *ngFor="let item of tablebox3.data">
                                                        <td *ngFor="let child of item"></td>
                                                    </tr>

                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                    <!-- 小路 -->
                                    <div class="flex-r wtable">
                                        <div class="iteminfo">
                                            {{tablebox4.name}}
                                        </div>
                                        <div class="wrapbox" [ngStyle]="{'width': tablebox4.width}">
                                            <div class="tablebox" id="tablebox4" [ngStyle]="{'left': tablebox4.left}" (mousedown)="dragdown($event,'tablebox4')" (mouseup)="tablebox4.drag = false"
                                                (mousemove)="dragmove($event,'tablebox4')" (mouseleave)="tablebox4.drag = false">
                                                <table id="table4" width="100%" border="0" cellspacing="0" cellpadding="4" bgcolor="#ccc" align="center">
                                                    <tr *ngFor="let item of tablebox4.data">
                                                        <td *ngFor="let child of item"></td>
                                                    </tr>

                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                    <!-- 曱甴路 -->
                                    <div class="flex-r wtable">
                                        <div class="iteminfo infosmall">
                                            {{tablebox5.name}}
                                        </div>
                                        <div class="wrapbox" [ngStyle]="{'width': tablebox5.width}">
                                            <div class="tablebox" id="tablebox5" [ngStyle]="{'left': tablebox5.left}" (mousedown)="dragdown($event,'tablebox5')" (mouseup)="tablebox5.drag = false"
                                                (mousemove)="dragmove($event,'tablebox5')" (mouseleave)="tablebox5.drag = false">
                                                <table id="table5" width="100%" border="0" cellspacing="0" cellpadding="4" bgcolor="#ccc" align="center">
                                                    <tr *ngFor="let item of tablebox5.data">
                                                        <td *ngFor="let child of item"></td>
                                                    </tr>

                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <!-- ========================================== -->
                        </div>

                    </div>

                </div>
                <!-- 右边信息区内容 -->
                <app-sider></app-sider>
            </div>
        </div>
    </div>