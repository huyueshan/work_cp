.flex-r {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-content: center;
    align-items: center;
}

.flex-c {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-content: flex-start;
    align-items: flex-start;
}

.color_y {
    color: #fe8f00;
    font-size: 12px;
}

.gamenav {
    // position: relative;
    width: 100%;
    color: #ccc;
    .padright {
        position: relative;
        padding-right: 20px;
        .allnav {
            position: absolute;
            top: 100px;
            left: 0;
            width: 240px;
            z-index: 999;
            .sidel {
                width: 100%;
                border: none;
                display: flex;
                flex-direction: column;
                justify-content: space-between;
                align-content: space-between;
                .flex-sr {
                    display: flex;
                    flex-direction: row;
                    justify-content: space-between;
                    align-content: center;
                    align-items: center;
                }
                .flex-sc {
                    display: flex;
                    flex-direction: column;
                    justify-content: space-between;
                    align-content: space-between;
                }
                li {
                    width: 100%;
                    font-size: 14px;
                    color: white;
                    .itembox {
                        font-size: 14px;
                        height: 42px;
                        line-height: 40px;
                        padding:0 12px 0 20px;
                        font-weight: bold;
                        background-color: #999;
                        border-bottom: 2px solid white;
                        cursor: pointer;
                        .itemboxiconr {
                            font-size: 30px;
                            line-height: 32px;
                            padding-top: 4px;
                            padding-right: 6px;
                            transform: rotate(0);
                        }
                        .iconactive {
                            line-height: 48px;
                            padding-right: 0;
                            transform: rotate(90deg);
                        }
                        .sideicon {
                            width: 36px;
                            height: 24px;
                            background: url('../../images/cred01.png');
                            background-repeat: no-repeat;
                        }
                    }
                    .itembox:hover {
                        background-color: #fe8f00;
                    }
                    .item {
                        font-size: 12px;
                        height: 32px;
                        line-height: 30px;
                        padding:0 18px 0 60px;
                        background-color: #525151;
                        color: white;
                        border-bottom: 1px solid white;
                        cursor: pointer;
                    }
                    .itemactive,
                    .item:hover {
                        color: #fe8f00;
                    }
                    .yborder {
                        background-color: #fe8f00;
                    }
                }
            }
        }

    }
    .navitem {
        height: 100px;
        background-color: #333;
        border-radius: 4px;
        overflow: hidden;
        .fltitle {
            width: 156px;
            line-height: 22px;
            padding-left: 30px;
        }
    }
    .alltype {
        
        width: 240px;
        .fltitle {
            background: #fe8f00;
            width: 40px;
            color: #fff;
            font-size: 14px;
            text-align: center;
            line-height: 20px;
            height: 100px;
            padding: 10px 10px;
            cursor: pointer;
        }
        .frimg {
            padding-left: 50px;
        }
    
    }
    .gametime {
        width: 340px;
        .frtime {
            text-align: center;
            padding-right: 10px;
            li {
                list-style: none;
                display: block;
                width: 50px;
                height: 50px;
                background: url("../../images/sscsprites.png") -53px -0;
                line-height: 50px;
                text-align: center;
                color: #fff;
                font-size: 16px;
            }
        }
    }
    .gameresult {
        width: 580px;
        .frnumber {
            padding-right: 10px;
            li {
                padding-right: 10px;
                span {
                    display: block;
                    width: 53px;
                    height: 53px;
                    background: url("../../images/sscsprites.png") -0 -0 no-repeat;
                    line-height: 50px;
                    text-align: center;
                    color: #fff;
                    font-size: 22px;
                }
            }
        }
    }
    .line {
        width: 2px;
        height: 52px;
        background: url("../../images/sscsprites.png") -161px -0;
    }
    .currresult {
        color: #fe8f00;
        font-size: 12px;
        line-height: 24px;
        width: 80px;
        height: 100px;
        justify-content: center;
        align-items: center;
        cursor: pointer;
        .pimg {
            span {
                display: inline-block;
                width: 14px;
                height: 15px;
                background: url("../../images/sscsprites.png") -141px -36px;
            }
        }
    }
}