*{
    box-sizing: border-box;
}
.flex-r {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-content: center;
    align-items: center;
}

.flex-c {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-content: space-between;
}

.bg_1 {
    background-color: #f5f5f5;
}

.container {
    width: 100%;
    padding: 0;
    align-content: flex-start;
    align-items: flex-start;
}

input {
  
    width: 132px;
    height: 30px;
    line-height: 20px;
    padding: 5px 0 4px 5px;
    margin: 0 5px;
    border: 1px solid #ccc;
}
input[type="checkbox"] {
    width: 16px;
    height: 16px;
    border: 1px solid #ccc;
    margin-top: 0;
}

#wrap {
    margin-right: 5px;
}

select {
    width: 100px;
    height: 30px;
    line-height: 20px;
    border: 1px solid #ccc;
}

.querycontent {
    width: 100%;
    height: 50px;
    padding: 0 10px;
    /* border: 1px solid #ccc; */
    /* border-radius: 3px; */
    /* background-color: #f5f5f5; */
    margin-top: 40px;

}
.querycontent>span{
     margin-right: 15px;
}
.querycontent select{
    min-width: 120px;
    height: 28px;
}
.querycontent label{
    color: #989898;
}
.querybtn {
    width: 70px;
    height: 30px;
    line-height: 28px;
    text-align: center;
    color: white;
    background: -webkit-linear-gradient(#D63C3E, #B52036); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#D63C3E, #B52036); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#D63C3E, #B52036); /* Firefox 3.6 - 15 */
    background: linear-gradient(#D63C3E, #B52036); /* 标准的语法 */
    border: 1px solid transparent;
    border-radius: 4px;
    cursor: pointer;
    
}


.tablebox {
    width: 100%;
    padding: 10px 0 0;
}

.tablewrap {
    width: 98%;
    margin: 0 auto;
    overflow-x: hidden;
    
    border: 1px solid #F5F5F5;
    border-top: 2px solid #636363;
}
#table2{
  width: 98%;
  margin: 0 1% ;
}
#get_more{
    height: 40px;
    width: 100%;
    font-size: 16px;
    text-align: center;
}
#get_more td{
    background: url("../../../images/user_center/more.png") center left 47% no-repeat ;
    text-align: center!important;
}
#table {
    table-layout: fixed;
    /* border: 1px solid #ccc; */
    border-collapse: collapse;
    width: 100%;
    border: none;
}

#table th,
    {
    font-size: 14px;
    font-weight: bold;
    text-align: left;
    color: #666666;
}

#table th,
#table td {
    text-align: left;
    text-indent: 15px;
    height: 40px;
    /* border: 1px solid #ccc; */
}
#table td{
   color: #333333 
}
#table tr:nth-child(odd){
   background-color: #F5F5F5;
}
#table tr:nth-last-child(1){
    cursor: pointer;
    background-color: #F5F5F5;
}
.page {
    align-self: center;

}
.viewbox{
    width:100%;
    padding: 20px 0;
}
.draw{
    width:100%;
    height: 400px;
    background-color: #f5f5f5;
    border: 1px solid #ccc;
    // 临时样式
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 50px;
    font-weight: bold;
}
.btn11{
    width: 74px;
    height: 30px;
    text-align: center;
    line-height: 28px;
    margin-right: 4px;
    display: inline-block;
    border-radius: 3px;
    color: white;
    cursor: pointer;
    background: -webkit-linear-gradient(#f9f9f9, #eee); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#f9f9f9, #eee); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#f9f9f9, #eee); /* Firefox 3.6 - 15 */
    background: linear-gradient(#f9f9f9, #eee); /* 标准的语法 */
    border: 1px solid #AFAFAF;
    color: #AFAFAF;
}
#page{
    width: auto;
    min-width: 40px;
    position: absolute;
    right: 200px;
}
.change_page{
        width: 160px;
        height: 30px;
        border:1px solid transparent;
    div{
        width: 50%;
        display: inline-block;
        font-size: 14px;
        text-align: center;
        height: 100%;
        line-height: 25px;
        cursor: pointer;
    }
    .pre_page{
        border: 1px solid #C9C9C9;
        border-radius:  15px 0 0 15px;
        background-color: #E5E5E5;
        color: #CBCBCB;

    }
    .next_page{
        border: 1px solid #939393;
        border-radius: 0 15px 15px 0;
        color: #666666;
        background: -webkit-linear-gradient(#f9f9f9, #eee); /* Safari 5.1 - 6.0 */
        background: -o-linear-gradient(#f9f9f9, #eee); /* Opera 11.1 - 12.0 */
        background: -moz-linear-gradient(#f9f9f9, #eee); /* Firefox 3.6 - 15 */
        background: linear-gradient(#f9f9f9, #eee); /* 标准的语法 */
    }

}
