
* {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    font-size: 12px;
    box-sizing: border-box;
}

.flex-r {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-content: center;
    align-items: center;
}

.flex-c {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-content: flex-start;
    align-items: flex-start;
}

.pagebox{
    .info{
        padding-right: 10px;
    }
    ul{
        list-style-type:none;
        li{
            padding: 0 4px;
            color: #666;
            button{
                width:28px;
                height: 28px;
                font-size: 12px;
                border: 1px solid #666;
                border-radius: 2px;
                background-color: #f5f5f5;
                outline: none;
                cursor: pointer;
            }
            .next{
                width:40px;
            }
            .active{
                color: #fe8f00;
                border-color: #fe8f00;

            }
            .disab{
                color: #bbb;
                border-color: #bbb;
                cursor: default ;
            }
            .dian{
                
                width:20px;
                height: 10px;
                line-height: 10px;
                margin-top: -12px;
                font-size: 22px;
                justify-content: center;
            }
        }
    }
}
.nopage{
    width: 100%;
    padding-top: 30px;
    justify-content: center;
    p{
        font-size: 14px;
        color:#333;
    }
}
