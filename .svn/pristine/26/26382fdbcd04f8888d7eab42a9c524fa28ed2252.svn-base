* {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    // font-size: 12px;
    box-sizing: border-box;
}

.flex-c {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-content: flex-start;
    align-items: center;
}

.flex-r {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-content: center;
    align-items: center;
}
a{
    text-decoration: none;
}
.color_m{
    color: #b61010;

}
.color_y {
    color: #fcdf0c;
}

.mycontainer {
    width: 100%;
    min-width: 1200px;
    background-color: #e5e2de;
    padding-bottom: 20px;
    
}

.head {
    width: 100%;
    min-width: 1200px;
    height: 96px;
    padding-bottom: 10px;
    .headbox {
        width: 100%;
        height: 76px;
        background: url("../images/head_top/menu-bg.png");
        background-size: cover;
        // background: url("../images/mod2/topbg.jpg") repeat;
        border-bottom: 2px solid #b61010;
        justify-content: center;
        .headcon {
            width: 1200px;
            height: 74px;
            line-height: 72px;
            font-size: 14px;
            color: white;
            justify-content: space-between;
            .logobox {
                position: relative;
                align-self: flex-start;
                .logo {
                    position: absolute;
                    top: 0;
                    left: 0px;
                    width: 185px;
                    height: 85px;
                    cursor: pointer;
                }
            }
            .head_r{
                position: relative;
                span {
                    padding: 0 5px;
                    i {
                        display: inline-block;
                    }
                    a{
                        color: white;
                    }
                    a:hover{
                        color: #fcdf0c;
                    }
                }
                .hello {
                    cursor: pointer;
                    i {
                        width: 16px;
                        height: 8px;
                        transform: rotate(180deg);
                        span {
                            display: inline-block;
                            width: 12px;
                            height: 8px;
                            margin-bottom: 4px;
                            background: url('../images/mod2/nav01.png') -10px -10px;
                            transform: scale(.6);
                            cursor: pointer;
                        }
                    }
                }
                .tolot {
                    cursor: pointer;
                    i {
                        width: 22px;
                        height: 16px;
                        background: url('../images/mod2/nav01.png') -10px -130px;
                    }
                }
                .tolot:hover,.rule:hover{
                    color: #fcdf0c;
                }
                .rule{
                    cursor: pointer;
                    i {
                        width: 20px;
                        height: 16px;
                        background: url('../images/mod2/nav01.png') -10px -166px;
                    }
                }
                .line{
                    line-height: 30px;
                    font-size: 18px;
                }
                .navbox{
                    position: absolute;
                    top:72px;
                    left: 0;
                    width: 362px;
                    background-color: white;
                    border: 1px solid #ccc;
                    z-index: 888;
                    .navwrap{
                        position: relative;
                        width: 100%;
                        height: 100%;
                        color: #333;
                        i {
                            position: absolute;
                            top: -4px;
                            left: 80px;
                            width: 6px;
                            height: 6px;
                            border-left: 1px solid #ccc;
                            border-top: 1px solid #ccc;
                            background-color: white;
                            transform: rotate(45deg);
                        }
                        ul{
                            width: 100%;
                            justify-content: flex-start;
                            padding-top: 20px;
                            flex-wrap: wrap;
                            li{
                                width: 80px;
                                margin:0 20px 20px 20px;
                                justify-content: center;
                                cursor: pointer;
                                .navicon{
                                    width: 68px;
                                    height: 68px;
                                    background: url('../images/mod2/nav02.png') -10px -10px;
                                }
                                footer{
                                    height:24px;
                                    line-height: 24px;
                                    font-size: 12px;
                                    text-align: center;
                                }
                            }
                            li:hover{
                                color:#b61010;
                            }
                        }
                        .foot{
                            width:320px;
                            height: 40px;
                            line-height: 32px;
                            font-size: 14px;
                            color:#2D73D2;
                            border-top: 1px solid #ccc;
                            justify-content: center;
                            cursor: pointer;
                        }
                        .foot:hover{
                            color:#b61010;
                        }
                    }

                }
            }
        }
    }
}
.main{
    width: 1200px;
    box-shadow: 0 2px 8px #333;
    align-items: flex-start;
    background-color: #fff;
    .sidebox{
        width:200px;
        // height: 100%;
        border-right: 1px solid #dadada;
        background-color: #eeeff0;  
        align-self: stretch;
        .sidel {
            width: 100%;
            border: none;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            align-content: space-between;
            .flex-sr {
                display: flex;
                flex-direction: row;
                justify-content: space-between;
                align-content: center;
                align-items: center;
            }
            .flex-sc {
                display: flex;
                flex-direction: column;
                justify-content: space-between;
                align-content: space-between;
            }
            li {
                width: 100%;
                font-size: 14px;
                color: #333333;             
                .itembox {
                    font-size: 14px;
                    // height: 42px;
                    // line-height: 40px;
                    padding:0 10px;
                    font-weight: bold;
                    cursor: pointer;
                    .borderb{
                        width: 100%;
                        height: 58px;
                        line-height: 56px;
                        border-top: 1px dashed #dadada;
                        .itemboxiconr {
                            width: 12px;
                            height: 8px;
                            background: url('../images/mod2/nav01.png') -10px -38px;
                        }
                        .iconactive {
                            background: url('../images/mod2/nav01.png') -10px -10px;
                        }
                        .sideicon {
                            width: 44px;
                            height: 44px;
                            background: url('../images/mod2/caizhong.png');
                            background-position-x:-10px;
                            background-repeat: no-repeat;
                        }
                    }
                }
                .itembox:hover {
                    color: white;
                    background-color: #b61010;
                    .borderb{
                        border: none !important;
                    }
                }
                .item {
                    font-size: 14px;
                    height: 40px;
                    line-height: 38px;
                    padding:0 18px 0 30px;
                    background-color: #eeeff0;
                    border-left: 2px solid #eeeff0;
                    color: #333;
                    cursor: pointer;
                    i{
                        display: inline-block;
                        width: 12px;
                        height: 12px;
                        background: url('../images/mod2/nav01.png') -10px -66px;
                        margin-right: 10px;
                    }
                }
                .itemactive,
                .item:hover {
                    color: #b61010;
                    border-left: 2px solid #b61010;
                    background-color: #f7f7f7;
                    i{
                        background: url('../images/mod2/nav01.png') -10px -98px;
                    }
                }
                .boxactive {
                    color: white;
                    background-color: #b61010;
                    .borderb{
                        border: none !important;
                    }
                }
            }
            li:last-child .itembox .borderb{
                border-bottom: 1px dashed #dadada;
            }
            li:first-child .itembox .borderb{
                border-top: none;
            }
            li:first-child .itembox .borderb{
                border-top: none;
            }
        }
    }
    .mainbox{
        width:1000px;
        align-self: stretch;
        .top{
            width: 100%;
            height: 140px;
            font-size: 14px;
            background-color: white;
            .topl{
                width:440px;
                height: 100%;
                border-right: 1px solid #dadada;
                .logoicon{
                    width:140px;
                    justify-content: center;
                    align-items: center;
                }
                .timeinfo{
                    height: 100%;
                    padding:20px 0;
                    justify-content: space-between;
                    align-items: flex-start;
                    .title1{
                        strong{
                            font-size: 18px;
                            line-height: 30px;
                            margin-right: 20px;
                        }
                        span{
                            padding: 2px 5px;
                            // font-size: 14px;
                            color: white;
                            background-color: #b61010;
                            border-radius: 4px;
                            cursor: pointer;

                        }
                    }
                    .title2{
                        // font-size: 14px;
                        span{
                            font-size: 14px;
                            padding: 0 5px;
                            font-weight: bold;
                        }
                    }
                    .timebar{
                        position: relative;
                        width:260px;
                        height: 16px;
                        border: none;
                        border-radius: 8px;
                        background-color: #D1D1D1;
                        #bar{
                            position: absolute;
                            // width:120px;
                            height: 16px;
                            border: none;
                            border-radius: 8px;
                            background: linear-gradient(to right, #ff0000, #ff8c39);

                        }

                    }
                    .title4{
                        // font-size: 14px;
                        i{
                            width: 18px;
                            height: 18px;
                            background: url('../images/mod2/icon_01.png') -10px -179px;
                            margin-right: 5px;
                        }
                        ul{
                            font-weight: bold;
                        }

                    }
                }
            }
            .topm{
                width:380px;
                height: 100%;
                align-items: flex-start;
                padding: 20px 10px;
                justify-content: center;
                .topmhead{
                    width: 100%;
                    padding:0 10px 10px 10px;
                    justify-content: space-between;
                    span{
                        font-size: 14px;
                        line-height: 30px;
                        .color_m{
                            font-weight: bold;
                            padding: 0 5px;
                        }
                    }
                    .bells{
                        width:100px;
                        height: 20px;
                        line-height: 20px;
                        font-size: 12px;
                        background-color: #EDEDED;
                        border-radius: 2px;
                        i{
                            width: 12px;
                            height: 12px;
                            margin: 0 4px;
                            background: url('../images/mod2/icon_01.png') -10px -10px;
                            cursor: pointer;
                        }
                        .bell{
                            background-position-y: -144px;
                        }
                        select{
                            width:76px;
                            height: 20px;
                            line-height: 320px;
                            font-size: 10px;
                            color: #333;
                            background-color: #EDEDED;
                            border: none;

                        }

                    }

                }
                .resultball{
                    width:100%;
                    justify-content: space-around;
                    flex-wrap: wrap;
                    i{
                        display: inline-block;
                        width:56px;
                        height: 52px;
                        line-height: 46px;
                        font-size: 28px;
                        text-align: center;
                        // margin:5px;
                        color: white;
                        background: url('../images/mod2/ball.png') -10px -348px;
                    }
                    .bg_r52{
                        background: url('../images/mod2/ball.png') -10px -348px;
                    }
                    .bg_b52{
                        background: url('../images/mod2/ball.png') -10px -492px;
                    }
                    .bg_g52{
                        background: url('../images/mod2/ball.png') -10px -420px;
                    }
                    .bg_gray52{
                        background: url('../images/mod2/ball.png') -10px -564px;
                    }
                    .bg_r42{
                        background: url('../images/mod2/lhball.png');
                    }
                    .bg_b42{
                        background: url('../images/mod2/ball.png') -10px -291px;
                    }
                    .bg_g42{
                        background: url('../images/mod2/ball.png') -10px -234px;
                    }
                    .bg_r24{
                        background: url('../images/mod2/ball.png') -10px -10px;
                    }
                    .bg_b24{
                        background: url('../images/mod2/ball.png') -10px -98px;
                    }
                    .bg_g24{
                        background: url('../images/mod2/ball.png') -10px -54px;
                    }
                    .bg_gray24{
                        background: url('../images/mod2/ball.png') -10px -142px;
                    }
                }
                .resultball2{
                    i{
                        width:32px;
                        height: 28px;
                        line-height: 24px;
                        font-size: 14px;
                        margin: 2px;
                        background: url('../images/mod2/ball.png') -10px -186px;
                    }
                }
                .resultball3{
                    span{
                        font-size: 22px;
                        font-weight: bold;
                    }
                }
                .resultball4{
                    i{
                        width:42px;
                        height: 38px;
                        line-height: 34px;
                        font-size: 18px;
                        background: url('../images/mod2/lhball.png') no-repeat;
                    }
                    span{
                        font-weight: bold;
                    }
                    .jia{
                        font-size: 22px;
                        font-weight: bold;
                        padding-bottom: 24px;

                    }

                }
            }
            .topr{
                height: 100%;
                width:180px;
                .hosthead{
                    width: 100%;
                    height: 30px;
                    flex: 0 0 30px;
                    line-height: 30px;
                    padding: 0 10px;
                    color: white;
                    background-color: #3b3b3b;
                    justify-content: space-between;
                    cursor: pointer;
                    .trend{
                        i{
                            display: inline-block;
                            width: 14px;
                            height: 14px;
                            margin-right: 4px;
                            background: url('../images/mod2/icon_01.png') -10px -42px;

                        }
                    }
                }
                ul{
                    width: 100%;
                    height: 100%;
                    overflow-y: auto;
                    border-left: 1px solid #dadada;
                    li{
                        width: 100%;
                        height: 22px;
                        flex: 0 0 22px;
                        line-height: 22px;
                        font-size: 10px;
                        padding: 0 10px;
                        justify-content: space-between;
                    }
                    li:nth-child(even){
                        background-color: #f4f4f4;
                    }

                }

            }

        }
        .gamebox{
            flex: auto;
            min-height: 700px;
        }
    }
}