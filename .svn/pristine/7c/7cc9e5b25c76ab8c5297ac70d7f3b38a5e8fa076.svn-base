* {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    // font-size: 12px;
    box-sizing: border-box;
}

.flex-c {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-content: flex-start;
    align-items: center;
}

.flex-r {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-content: center;
    align-items: center;
}
a{
    text-decoration: none;
}
.color_m{
    color: #b61010;

}
.color_y {
    color: #fcdf0c;
}

.mycontainer {
    width: 100%;
    background-color: #e5e2de;
    padding-bottom: 20px;
    
}

.head {
    width: 100%;
    min-width: 1200px;
    height: 96px;
    padding-bottom: 10px;
    .headbox {
        width: 100%;
        height: 76px;
        background-color: #101010;
        border-bottom: 2px solid #b61010;
        justify-content: center;
        .headcon {
            width: 1200px;
            height: 74px;
            line-height: 72px;
            font-size: 14px;
            color: white;
            justify-content: space-between;
            .logobox {
                position: relative;
                align-self: flex-start;
                .logo {
                    position: absolute;
                    top: 0;
                    left: 0px;
                    width: 185px;
                    height: 85px;
                    cursor: pointer;
                }
            }
            span {
                padding: 0 5px;
                i {
                    display: inline-block;
                }
                a{
                    color: white;
                }
                a:hover{
                    color: #fcdf0c;
                }
            }
            .hello {
                i {
                    width: 16px;
                    height: 8px;
                    transform: rotate(180deg);
                    span {
                        display: inline-block;
                        width: 12px;
                        height: 8px;
                        margin-bottom: 4px;
                        background: url('../images/mod2/nav01.png') -10px -10px;
                        transform: scale(.6);
                        cursor: pointer;
                    }
                }
            }
            .tolot {
                cursor: pointer;
                i {
                    width: 22px;
                    height: 16px;
                    background: url('../images/mod2/nav01.png') -10px -130px;
                }
            }
            .tolot:hover,.rule:hover{
                color: #fcdf0c;
            }
            .rule{
                cursor: pointer;
                i {
                    width: 20px;
                    height: 16px;
                    background: url('../images/mod2/nav01.png') -10px -166px;
                }
            }
            .line{
                line-height: 30px;
                font-size: 18px;
            }
        }
    }
}
.main{
    width: 1200px;
    box-shadow: 0 2px 8px #333;
    align-items: flex-start;
    background-color: #fff;
    .sidebox{
        width:200px;
        // height: 100%;
        border-right: 1px solid #dadada;
        background-color: #eeeff0;  
        align-self: stretch;
        .sidel {
            width: 100%;
            border: none;
            display: flex;
            flex-direction: column;
            justify-content: space-between;
            align-content: space-between;
            .flex-sr {
                display: flex;
                flex-direction: row;
                justify-content: space-between;
                align-content: center;
                align-items: center;
            }
            .flex-sc {
                display: flex;
                flex-direction: column;
                justify-content: space-between;
                align-content: space-between;
            }
            li {
                width: 100%;
                font-size: 14px;
                color: #333333;             
                .itembox {
                    font-size: 14px;
                    // height: 42px;
                    // line-height: 40px;
                    padding:0 10px;
                    font-weight: bold;
                    cursor: pointer;
                    .borderb{
                        width: 100%;
                        height: 58px;
                        line-height: 56px;
                        border-top: 1px dashed #dadada;
                        .itemboxiconr {
                            width: 12px;
                            height: 8px;
                            background: url('../images/mod2/nav01.png') -10px -38px;
                        }
                        .iconactive {
                            background: url('../images/mod2/nav01.png') -10px -10px;
                        }
                        .sideicon {
                            width: 44px;
                            height: 44px;
                            background: url('../images/mod2/caizhong.png');
                            background-position-x:-10px;
                            background-repeat: no-repeat;
                        }
                    }
                }
                .itembox:hover {
                    color: white;
                    background-color: #b61010;
                    .borderb{
                        border: none !important;
                    }
                }
                .item {
                    font-size: 14px;
                    height: 40px;
                    line-height: 38px;
                    padding:0 18px 0 30px;
                    background-color: #eeeff0;
                    border-left: 2px solid #eeeff0;
                    color: #333;
                    cursor: pointer;
                    i{
                        display: inline-block;
                        width: 12px;
                        height: 12px;
                        background: url('../images/mod2/nav01.png') -10px -66px;
                        margin-right: 10px;
                    }
                }
                .itemactive,
                .item:hover {
                    color: #b61010;
                    border-left: 2px solid #b61010;
                    background-color: #f7f7f7;
                    i{
                        background: url('../images/mod2/nav01.png') -10px -98px;
                    }
                }
                .boxactive {
                    color: white;
                    background-color: #b61010;
                    .borderb{
                        border: none !important;
                    }
                }
            }
            li:last-child .itembox .borderb{
                border-bottom: 1px dashed #dadada;
            }
            li:first-child .itembox .borderb{
                border-top: none;
            }
            li:first-child .itembox .borderb{
                border-top: none;
            }
        }
    }
    .mainbox{
        width:1000px;
        align-self: stretch;
        .top{
            width: 100%;
            height: 140px;
            font-size: 14px;
            background-color: white;
            .topl{
                width:440px;
                height: 100%;
                border-right: 1px solid #dadada;
                .logo{
                    width:140px;
                    justify-content: center;
                }
                .timeinfo{
                    height: 100%;
                    padding:20px 0;
                    justify-content: space-between;
                    align-items: flex-start;
                    .title1{
                        strong{
                            font-size: 18px;
                            line-height: 30px;
                            margin-right: 20px;
                        }
                        span{
                            padding: 2px 5px;
                            // font-size: 14px;
                            color: white;
                            background-color: #b61010;
                            border-radius: 4px;
                            cursor: pointer;

                        }
                    }
                    .title2{
                        // font-size: 14px;
                        span{
                            font-size: 14px;
                            padding: 0 5px;
                            font-weight: bold;
                        }
                    }
                    .timebar{
                        position: relative;
                        width:260px;
                        height: 16px;
                        border: none;
                        border-radius: 8px;
                        background-color: #D1D1D1;
                        #bar{
                            position: absolute;
                            // width:120px;
                            height: 16px;
                            border: none;
                            border-radius: 8px;
                            background: linear-gradient(to right, #ff0000, #ff8c39);

                        }

                    }
                    .title4{
                        // font-size: 14px;
                        i{
                            width: 18px;
                            height: 18px;
                            background: url('../images/mod2/icon_01.png') -10px -179px;
                            margin-right: 5px;
                        }
                        ul{
                            font-weight: bold;
                        }

                    }
                }
            }
            .topm{
                width:380px;
                height: 100%;
                // border-right: 1px solid #dadada;
                justify-content: center;
                align-items: flex-start;
                padding: 20px 10px;
                span{
                    font-size: 14px;
                    line-height: 30px;
                    .color_m{
                        font-weight: bold;
                        padding: 0 5px;
                    }
                }
                .resultball{
                    width:100%;
                    justify-content: space-around;
                    flex-wrap: wrap;
                    i{
                        display: inline-block;
                        width:56px;
                        height: 52px;
                        line-height: 42px;
                        font-size: 28px;
                        text-align: center;
                        margin:5px;
                        color: white;
                        background: url('../images/mod2/ball.png') -10px -348px;
                    }
                }
            }
            .topr{
                height: 100%;
                width:180px;
                .hosthead{
                    width: 100%;
                    height: 30px;
                    flex: 0 0 30px;
                    line-height: 30px;
                    padding: 0 10px;
                    color: white;
                    background-color: #3b3b3b;
                    justify-content: space-between;
                    cursor: pointer;
                    .trend{
                        i{
                            display: inline-block;
                            width: 14px;
                            height: 14px;
                            margin-right: 4px;
                            background: url('../images/mod2/icon_01.png') -10px -42px;

                        }
                    }
                }
                ul{
                    width: 100%;
                    height: 100%;
                    overflow-y: auto;
                    border-left: 1px solid #dadada;
                    li{
                        width: 100%;
                        height: 22px;
                        flex: 0 0 22px;
                        line-height: 22px;
                        font-size: 10px;
                        padding: 0 10px;
                        justify-content: space-between;
                    }
                    li:nth-child(even){
                        background-color: #f4f4f4;
                    }

                }

            }

        }
        .gamebox{
            // align-self: stretch;
            flex: auto;
        }
    }
}