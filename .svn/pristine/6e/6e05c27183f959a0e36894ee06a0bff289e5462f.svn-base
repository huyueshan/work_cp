<div class="flex-c sscbox" [hidden]="!loadpage">
    <div class="flex-c pccontain">
        <div class="contaner">
            <app-credtop [type]="'pk10'" [indata]="cpnav"></app-credtop>
            <div class="flex-r mainbox">
                <div class="flex-c contentbox">
                    <div class="flex-r tabbox">
                        <div class="flex-r tabcontent">
                            <span *ngFor="let item of typedata" [ngClass]="{tabactive:this.type == item.id}" (click)="togtype(item.id)">{{item.name}}</span>
                        </div>
                    </div>
                    <div class="flex-r selctbox" onselectstart="return false;" style="-moz-user-select:none;">
                        <span class="flex-r">
                            <strong>$</strong>
                            <span class="flex-r rangebox">
                                <span class="showvalue">{{(odds-rangevalue)| number : '1.0-1'}}%</span>
                                <span class="icon" (click)="rangevaluelessen()">-</span>
                                <span class="flex-r range">
                                    <input type="range" name="points" min="0" max="{{odds}}" step="0.1" id="points" [(ngModel)]="rangevalue">
                                </span>
                                <span class="icon" (click)="rangevalueadd()">+</span>
                            </span>
                        </span>
                        <span class="flex-r btngroup">
                            <button [ngClass]="{btnactive:selectbtnvalue === 0}" (click)="tabclick(0)">一般</button>
                            <button [ngClass]="{btnactive:selectbtnvalue === 1}" (click)="tabclick(1)">快捷</button>
                            <button class="kuxuan" (click)="tabclick(2)">快选金额</button>
                        </span>
                    </div>
                    <div class="flex-c mycontaion">

                        <!-- 特码 -->
                        <div class="flex-c content content1" *ngIf="type === 1">
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox" *ngFor="let item of contenttoptitle;">

                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">号码</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" *ngIf="inputshow">金额</span>
                                        </span>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-c conmain" style="height: 400px;">
                                <div class="flex-c conmainbox" *ngFor="let item of dpcdata1.data1; let i=index">
                                    <span class="flex-r itembox">
                                        <span class="flex-r item1 " *ngIf="item.numb !== null">
                                            <i [ngClass]="{bgred: item.red , bgblue: item.blue, bggreen: item.green }">{{item.numb}}</i>
                                        </span>
                                        <span class="item2 color_y" *ngIf="item.numb !== null">{{(11.633+(1.3/7.8*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" *ngIf="inputshow && item.numb !== null">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value" (focus)="inmoney1focus($event, 1,'data1', i,null)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </span>
                                </div>
                            </div>
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox" *ngFor="let item of dpcdata1.data2; let i=index">
                                    <span class="flex-r itembox" *ngFor="let child of item; let q=index">
                                        <span class="item1" *ngIf="child.name!==null">
                                            {{child.name}}
                                        </span>
                                        <span class="item2 color_y" *ngIf="child.name!==null">{{(11.633+(1.3/7.8*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" *ngIf="inputshow">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="child.value" (focus)="inmoney1focus($event, 1,'data2', i, q)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()" *ngIf="child.name!==null">
                                        </span>
                                    </span>
                                </div>
                            </div>


                        </div>
                        <!-- 色波 -->
                        <div class="flex-c content content1" *ngIf="type === 2">
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox conmainbox1">

                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">项目</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" *ngIf="inputshow">金额</span>
                                        </span>
                                        <span class="flex-r item4box">号码</span>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-c conmain">
                                <div class="flex-r conmainbox conmainbox1" *ngFor="let item of dpcdata2.data1; let i=index">
                                    <span class="flex-r itembox">
                                        <span class="flex-r item1 " *ngIf="item.name !== null">
                                            {{item.name}}
                                        </span>
                                        <span class="item2 color_y" *ngIf="item.name !== null">{{(11.633+(1.3/7.8*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" *ngIf="inputshow && item.name !== null">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value" (focus)="inmoney1focus($event, 2,'data1', i, null)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </span>
                                    <span class="flex-r item4box">
                                        <i *ngFor="let child of item.data" [ngClass]="{bgred: child.red , bgblue: child.blue, bggreen: child.green }">{{child.numb}}</i>
                                    </span>
                                </div>
                            </div>

                        </div>
                        <!-- 特肖 -->
                        <div class="flex-c content content1" *ngIf="type === 3">
                            <div class="flex-r conmain">
                                <div class="flex-r conmainbox conmainbox2">

                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">生肖</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" *ngIf="inputshow">金额</span>
                                        </span>
                                        <span class="flex-r item4box">号码</span>

                                    </div>
                                </div>
                                <div class="flex-r conmainbox conmainbox2">

                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">生肖</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" *ngIf="inputshow">金额</span>
                                        </span>
                                        <span class="flex-r item4box">号码</span>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-r conmain">
                                <div class="flex-r conmainbox conmainbox2" *ngFor="let item of dpcdata3.data1; let i=index">
                                    <span class="flex-r itembox">
                                        <span class="flex-r item1 " *ngIf="item.name !== null">
                                            {{item.name}}
                                        </span>
                                        <span class="item2 color_y" *ngIf="item.name !== null">{{(11.633+(1.3/7.8*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" *ngIf="inputshow && item.name !== null">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value" (focus)="inmoney1focus($event, 3,'data1', i, null)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </span>
                                    <span class="flex-r item4box">
                                        <i *ngFor="let child of item.data" [ngClass]="{bgred: child.red , bgblue: child.blue, bggreen: child.green }">{{child.numb}}</i>
                                    </span>
                                </div>
                            </div>

                        </div>
                        <!-- 正码 -->
                        <div class="flex-c content content1" *ngIf="type === 4">
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox" *ngFor="let item of contenttoptitle;">

                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">号码</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" *ngIf="inputshow">金额</span>
                                        </span>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-c conmain" style="height: 400px;">
                                <div class="flex-c conmainbox" *ngFor="let item of dpcdata4.data1; let i=index">
                                    <span class="flex-r itembox">
                                        <span class="flex-r item1 " *ngIf="item.numb !== null">
                                            <i [ngClass]="{bgred: item.red , bgblue: item.blue, bggreen: item.green }">{{item.numb}}</i>
                                        </span>
                                        <span class="item2 color_y" *ngIf="item.numb !== null">{{(11.633+(1.3/7.8*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" *ngIf="inputshow && item.numb !== null">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value" (focus)="inmoney1focus($event, 4,'data1', i,null)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </span>
                                </div>
                            </div>


                        </div>

                        <!-- 按钮 -->
                        <div class="flex-r subbox">
                            <span class="flex-r btngroup">
                                <span *ngIf="!inputshow">
                                    <input type="text" maxlength="9" class="inmoney" id="allinmoney" name="allinmoney" [(ngModel)]="setallmoney.value" value=""
                                        (focus)="inmoneyfocus($event,'all')" (blur)="inmoneyblur()" (change)="allchange()" (keyup)="changereg()">
                                </span>
                                <button class="btnactive" (click)="sub()">确定</button>
                                <button class="resetbtn" (click)="reset()">取消</button>
                            </span>

                        </div>

                        <!-- 活动选择框 -->
                        <div id="selectbox" [ngStyle]="{'top': boxposition.y, 'left': boxposition.x}" *ngIf="boxshow && boxvalid">
                            <div class="flex-c selectwrap">
                                <i></i>
                                <span class="option" *ngFor="let item of selmoeny" (mousedown)="optinclick(item)">{{item}}元</span>
                            </div>
                        </div>
                        <!-- 底部问路-->
                        <div *ngIf="type!==3">
                            <app-wenlu></app-wenlu>
                        </div>
                        <!-- 快选金额 -->
                        <div class="setbox" *ngIf="popup.setnumb.show">
                            <div class="flex-c content">
                                <div class="flex-r top">
                                    <span>快选金额</span>
                                    <i (click)="close()"> &times; </i>
                                </div>
                                <div class="flex-c main">
                                    <p>注意： </p>
                                    <p>设置只保存在您的电脑，清空浏览器缓存或 更换电脑会显示默认值。</p>
                                    <div class="flex-c setinput">

                                        <span class="flex-r item" *ngFor="let item of popup.setnumb.data; let i=index">
                                            <input type="text" name="setinput" maxlength="9" [(ngModel)]="popup.setnumb.data[i].value" (keyup)="changeregset(i)">
                                            <span (click)="setnumbdel(i)">删除</span>
                                        </span>
                                        <span class="flex-r item">
                                            <input type="text" name="setinput" maxlength="9" [(ngModel)]="popup.setnumb.value" (keyup)="changeregset(-1)">
                                            <span (click)="addnumb()">增加</span>
                                            <span (click)="numbdel()">删除</span>
                                        </span>

                                    </div>
                                    <div class="flex-r btnbox">
                                        <span class="btn1" (click)="setboxvalid()">{{boxvalid?'禁用':'开启'}}</span>
                                        <span class="btn2" (click)="savenum()">保存</span>
                                    </div>
                                </div>


                            </div>
                        </div>

                        <!-- 提交窗口 -->
                        <div class="subpopup" *ngIf="popup.sub.show" [ngStyle]="{'top': popup.sub.top}">
                            <div class="flex-c content">
                                <div class="flex-r top">
                                    <span>投注信息</span>
                                    <i (click)="close()"> &times; </i>
                                </div>
                                <div class="flex-c thead">
                                    <table id="table" width="100%" border="0" cellspacing="0" cellpadding="4" bgcolor="#ccc" align="center">
                                        <tr class="bg_1">
                                            <th style="width: 60px;">序号</th>
                                            <th style="width: 160px;">频道</th>
                                            <th style="width: 120px;">玩法</th>
                                            <th style="width: 100px;">期号</th>
                                            <th style="width: 60px;">球号</th>
                                            <th style="width: 60px;">号码</th>
                                            <th style="width: 80px;">赔率</th>
                                            <th style="padding-right: 18px;">金额</th>
                                        </tr>
                                    </table>
                                </div>
                                <div class="flex-c tablewrap">
                                    <table id="table" width="100%" border="0" cellspacing="0" cellpadding="4" bgcolor="#ccc" align="center">
                                        <tbody>
                                            <tr *ngFor="let item of subdata; let i=index">
                                                <td style="width: 60px;">{{ i + 1 }}</td>
                                                <td style="width: 160px;">{{item.channel}}</td>
                                                <td style="width: 120px;">{{item.type}}</td>
                                                <td style="width: 100px;">{{item.id}}</td>
                                                <td style="width: 60px;">{{item.ball}}</td>
                                                <td style="width: 60px;">{{item.number}}</td>
                                                <td style="width: 80px;">{{item.point}}</td>
                                                <td>{{item.money}}</td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>
                                <span class="flex-r foot">总计投注金额
                                    <strong>{{submoney}}</strong> 元 </span>


                            </div>

                        </div>

                        <!-- 遮罩层 -->
                        <div class="shade" [style.width.px]="popup.shade.w" [style.height.px]="popup.shade.h" *ngIf="popup.shade.show"></div>

                    </div>

                </div>
                <!-- 右边信息区内容 -->
                <app-sider></app-sider>
            </div>
        </div>
    </div>