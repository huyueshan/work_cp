<div id="layout">

    <div class="chart" [style.width.px]="canva.width">
        <table width=auto border="0" cellpadding="0" cellspacing="0" align="center" id="chartsTable">
            <tr class="thead">
                <td rowspan="2" class="td_first" style="width:60px" align="center">
                    <strong>期号</strong>
                </td>
                <td rowspan="2" colspan="5" class="td_second" align="center">
                    开奖号码
                </td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">万位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">千位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">百位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">十位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">个位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">号码分布</td>
            </tr>
            <tr class="thead">
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
            </tr>

            <tbody id="tdata">
                <tr *ngFor="let item of outdata" [ngClass]="{'trborder': viewdata.issubline}">
                    <td class="number">{{item.number}}</td>
                    <td *ngFor="let ball of item.data" class="bgred resultbg">
                        <span class="span">{{ball}}</span>
                    </td>
                    <ng-container *ngFor="let child1 of item.body; let i=index">
                        <td bgcolor="#333" class="line"></td>
                        <td *ngFor="let child2 of child1.data" class="{{child1.clname}}" [ngClass]="{
            'omissionbg':child2.isomission && viewdata.isomission,
            'bgred':child2.ischecked,
            'omissionbg':child2.isomission && viewdata.isomission
          }">
                            <span class="span" *ngIf="viewdata.isomissionshow && !child2.ischecked">{{child2.omisvalue}}</span>
                            <span class="span"
                            *ngIf="child2.ischecked"
                            [ngClass]="{'bgblue':child2.haowen===1 && viewdata.ishaowen,'bgyellow':child2.haowen===2 && viewdata.ishaowen,'bgred2':child2.haowen===3 && viewdata.ishaowen}" >{{child2.value}}</span>
                        </td>
                    </ng-container>
                    <ng-container>
                        <td bgcolor="#333" class="line"></td>
                        <td *ngFor="let child3 of item.disdata" [ngClass]="{
            'omissionbg':child3.isomission && viewdata.isomission,
            'bgred':child3.ischecked,
            'omissionbg':child3.isomission && viewdata.isomission
          }">
                            <span class="span" *ngIf="viewdata.isomissionshow && !child3.ischecked">{{child3.omisvalue}}</span>
                            <span class="span span2" [ngClass]="{'doubbg':child3.isdouble && viewdata.isdouble}" *ngIf="child3.ischecked">{{child3.value}}</span>
                        </td>
                    </ng-container>
                </tr>
            </tbody>
            <ng-container *ngIf="viewdata.isdisdata" >
                <tr style="color:#ff00ff;">
                    <td colspan="6">出现总次数</td>
                    <ng-container *ngFor="let item of stdata.sumv.data">
                        <td bgcolor="#333" class="line"></td>
                        <td *ngFor="let child of item">{{child.value}}</td>
                    </ng-container>
                    <td bgcolor="#333" class="line"></td>
                    <td *ngFor="let child of stdata.sumv.disdata">{{child.value}}</td>
                </tr>
                <tr style="color:#485a30;">
                    <td colspan="6">平均遗漏</td>
                    <ng-container *ngFor="let item of stdata.meanom.data">
                        <td bgcolor="#333" class="line"></td>
                        <td *ngFor="let child of item">{{child.value}}</td>
                    </ng-container>
                    <td bgcolor="#333" class="line"></td>
                    <td *ngFor="let child of stdata.meanom.disdata">{{child.value}}</td>
                </tr>
                <tr style="color:#803905;">
                    <td colspan="6">最大遗漏</td>
                    <ng-container *ngFor="let item of stdata.maxom.data">
                        <td bgcolor="#333" class="line"></td>
                        <td *ngFor="let child of item">{{child.value}}</td>
                    </ng-container>
                    <td bgcolor="#333" class="line"></td>
                    <td *ngFor="let child of stdata.maxom.disdata">{{child.value}}</td>
                </tr>
                <tr style="color:#18566b;">
                    <td colspan="6">最大连出</td>
                    <ng-container *ngFor="let item of stdata.maxre.data">
                        <td bgcolor="#333" class="line"></td>
                        <td *ngFor="let child of item">{{child.value}}</td>
                    </ng-container>
                    <td bgcolor="#333" class="line"></td>
                    <td *ngFor="let child of stdata.maxre.disdata">{{child.value}}</td>
                </tr>
            </ng-container>
            <tr class="thead">
                <td rowspan="2" class="td_first" style="width:60px" align="center">
                    <strong>期号</strong>
                </td>
                <td rowspan="2" colspan="5" class="td_second">
                    开奖号码
                </td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td *ngFor="let item of thdata">{{item}}</td>
            </tr>
            <tr class="thead">
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">万位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">千位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">百位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">十位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">个位</td>
                <td width="1" bgcolor="#333" class="line"></td>
                <td colspan="10">号码分布</td>
            </tr>
            <div class="canwrap" [hidden]="!viewdata.isshowline">
                <canvas id="canva" width="{{canva.width}}" height="{{canva.height}}"></canvas>
            </div>
        </table>
    </div>
</div>