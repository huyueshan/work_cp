@charset "UTF-8";
@import '../../images/common';
html,body {height:100%;}
$yellow:#fe8f00;
.nav{
	width:100%;
	position:relative;
	float:left;
}
*{
	-webkit-user-select:none;
	-moz-user-select:none;
	-ms-user-select:none;
	user-select:none;
	font-size:12px;
}
/* 颜色 */
.orange_font{
	color: #fe8f00;
}
.jiangjin_tips{
	float: left;
    position: relative;
    z-index: 998;
    width: 100px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    font-size: 14px;
    background-color: rgb(204, 204, 204);
    color: rgb(68, 68, 68);
    border-radius: 5px;
    cursor: pointer;
    margin-left: 10px;
    margin-top: 5px;
}
/* 提示 */
.tips{
	width: 250px;
	/* min-height: 74px; */
	padding: 10px 25px;
	background-color: #222;
	line-height: 25px;
	position: absolute;
	left: 0;
	top:0;
	color: white;
	/* margin-top: -125%; */
	transform:translateY(-110%) scale(0.5);
	margin-left: -125px;
	border:2px solid #fe8f00;
	border-radius: 4px;
	text-align: center;
	transition:0.1s ease;
}
.long_tips{
	text-align: left;
	width: 400px;
}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    -webkit-appearance: none;
}
input[type="number"]{
    -moz-appearance: textfield;
}
.dropdown{
	height:auto;
	width: 50px;
	position: absolute;
	margin-left:56px;
	border:1px solid #ccc;
	box-shadow: 0px -2px 8px 1px rgba(0,0,0,0.2);
	margin-top: 22px;
	background-color: #fff;
	li{
		border-bottom: 1px solid #ccc;
		text-align: center;
		cursor: pointer;
	}
	li:hover{
		background-color: #eee;
	}
}
.ulhidden{
	display: none;
}
.tipshidden{
	/* display: none; */
	visibility: hidden;
}


.tipsshow{
	/* display: block; */
	visibility: visible;
	transform:translateY(-110%) scale(1);
}
.check_ballplace{
	height: 35px;
	width: 100%;
	background-color: #fff;
	clear: both;
	>div{
		height: 100%;
		line-height: 35px;
		vertical-align: top;
		font-size: 14px;
		float: left;
	}
	.til{
		text-indent: 38px;
		width: 15%;
	}
	.check_box{
		width: 25%;
	}
	.check_box input{
		margin-top: 10px;
		margin-right: 5px;
		vertical-align: top;
	}
	.tips_right{
		width: 60%;
	}
	.tips_right>div{
		display: inline-block;
	}
}

