input[type="range"] {
  display: inline-block;
  position: relative;
  width: 90px;
  height: 22px;
  background: none;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  cursor: pointer;
  outline: none;
  margin:0px 5px;
}

/*range中心圆点*/

input[type="range"]::-webkit-slider-thumb {
  -webkit-appearance: none;
  appearance: none;
  height: 12px;
  width: 18px;
  border-radius: 5px;
  background-color: #fe8f00;
  cursor: pointer;
  margin-top: -4px;
}

input[type="range"]::-moz-range-thumb {
  -moz-appearance: none;
  appearance: none;
  height: 12px;
  width: 18px;
  border-radius: 5px;
  background-color: #fe8f00;
  cursor: pointer;
}
input[type="range"]::-ms-thumb {
  -ms-appearance: none;
  appearance: none;
  height: 12px;
  width: 18px;
  border-radius: 5px;
  background-color: #fe8f00;
  cursor: pointer;
  margin-top: 1px;
}
input[type="range"]::slider-thumb {
  -webkit-appearance: none;
  appearance: none;
  height: 12px;
  width: 18px;
  border-radius: 5px;
  background-color: #fe8f00;
  cursor: pointer;
  margin-top: -6px;
}
/*range轨道*/
input[type="range"]::-webkit-slider-runnable-track {
  width: 100px;
  height: 4px;
  background-color: #fe8f00;
  border-radius: 2px;
  cursor: pointer;
}
input[type="range"]::-moz-range-track {
  width: 100px;
  height: 4px;
  background-color: #fe8f00;
  border-radius: 2px;
  cursor: pointer;
}
input[type="range"]::-ms-track {
  width: 100px;
  height: 4px;
  border-color: transparent;
  color: #fe8f00;
  border-radius: 2px;
  background: #fe8f00;
  cursor: pointer;
}

