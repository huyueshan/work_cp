.pccontain {
    width: 100%;
    align-self: center;
    background: #111 url(../../images/bg_02.jpg) center no-repeat;
    background-size: cover;
    .contaner {
        width: 1200px;
        align-self: center;
        padding-top: 30px;
        .mainbox {
            padding: 20px 0;
            justify-content: space-between;
            align-content: flex-start;
            align-items: flex-start;
            .contentbox {
                width: 900px;
                border-radius: 5px;
                background-color: #ddd;
                align-self: stretch;
                .tabbox {
                    width: 100%;
                    padding-right: 20px;
                    justify-content: space-between;
                    background-color: #333;
                    border-top-left-radius: 4px;
                    border-top-right-radius: 4px;
                    overflow: hidden;
                    .tabcontent {
                        span {
                            height: 50px;
                            line-height: 50px;
                            padding: 0 18px;
                            color: white;
                            cursor: pointer;
                        }
                        .tabactive {
                            background-color: #3d3d3d;
                        }
                    }
                    .togglebtn {
                        width: 90px;
                        height: 25px;
                        line-height: 18px;
                        border: 1px solid white;
                        border-radius: 3px;
                        color: white;
                        background-color: #333;
                        overflow: hidden;
                        span {
                            width: 50%;
                            height: 100%;
                            line-height: 24px;
                            text-align: center;
                            cursor: pointer;
                        }
                        .togactive,
                        span:hover {
                            color: #111;
                            background-color: white;
                        }
                    }
                }
                .selctbox {
                    width: 100%;
                    height: 60px;
                    padding: 0 20px 0 30px;
                    justify-content: space-between;
                    background-color: #3d3d3d;
                    strong {
                        width: 30px;
                        height: 30px;
                        line-height: 30px;
                        font-size: 20px;
                        text-align: center;
                        border-radius: 50%;
                        background-color: #fe8f00;
                        color: white;
                    }
                    .rangebox {
                        color: white;
                        .showvalue {
                            flex: 0 0 70px;
                            width: 50px;
                            text-align: right;
                            padding-right: 20px;
                        }
                        .icon {
                            flex: none;
                            width: 16px;
                            height: 16px;
                            font-size: 14px;
                            line-height: 14px;
                            text-align: center;
                            font-weight: bold;
                            border-radius: 50%;
                            background-color: #999;
                            color: white;
                            cursor: pointer;
                        }
                        .range {
                            width: 130px;
                            padding: 0 10px;
                            align-items: center;
                        }
                    }
                    .btngroup {
                        button {
                            width: 80px;
                            font-size: 14px;
                            height: 32px;
                            line-height: 30px;
                            text-align: center;
                            padding: 0;
                            background-color: #ccc;
                            color: #333;
                            border: 1px solid #999;
                            border-radius: 3px;
                            margin-left: 10px;
                            outline: none;
                        }
                        .lactive {
                            margin-left: 0px;
                        }
                        .btnactive,
                        .resetbtn:hover,
                        .kuxuan:hover {
                            color: white;
                            background-color: #fe8f00;
                            background: linear-gradient(to top, #c76401, #fba915);
                            border: 1px solid #d17702;
                            outline: none;
                        }
                    }
                }
            }
        }
    }
}

.mycontaion {
    position: relative;
    width: 900px;
    .content {
        width: 100%;
        align-items: center;
        .contenttop {
            width: 100%;
            background-color: #eee;
            .itembox {
                flex: 0 0 20%;
                height: 40px;
                border-right: 1px solid #ddd;
                span {
                    text-align: center;
                    border-right: 1px solid #ddd;
                }
                span:last-child {
                    border-right: none;
                }
                .item1,
                .item2 {
                    flex: 3;
                    line-height: 24px;
                }
                .item3 {
                    flex: 4;
                }
            }
            .itembox:last-child {
                border-right: none;
            }
        }
        .conmain {
            width: 100%;
            background-color: white;
            flex-wrap: wrap;
            background-color: white;
            .itembox {
                flex: 0 0 20%;
                height: 40px;
                border-right: 1px solid #ddd;
                border-bottom: 1px solid #ddd;
                span {
                    text-align: center;
                }
                .item1,
                .item2 {
                    flex: 3;
                    line-height: 24px;
                }
                .item3 {
                    flex: 4;
                }
                .item1 {
                    span {
                        display: inline-block;
                        height: 26px;
                        line-height: 26px;
                        text-align: center;
                        font-weight: bold;
                    }
                }
            }
        }
        
    }
}

