<div class="container">
        
        <div class="head_box">
            <div class="head">
                <div class="head_top">
                    <div class="log_ico">
                        
                    </div>
                    <div class="flex-r fl-r head_top_right" >
                        <div class="accountbox" [ngClass]="{errbd:logindata.name.err}">
                            <label for="account"></label>
                            <input type="text"
                            name="name"
                            id="account"
                            placeholder={{now_lang.index.User_pho}}
                            [(ngModel)]="logindata.name.value"
                            (focus)="focus('name')" />
                        </div>
                        <div class="passbox" [ngClass]="{errbd:logindata.password.err}">
                            <label for="password"></label>
                            <input type="password"
                            name="password"
                            id="password"
                            placeholder={{now_lang.index.Password}}
                            [(ngModel)]="logindata.password.value"
                            (focus)="focus('password')" />
                            <span class="forgetbtn" (click)="linkrouter('/usercenter/security')">{{now_lang.index.Forget}}?</span>
                        </div>
                        <div class="secubox flex-r" [ngClass]="{errbd:logindata.security.err}">
                            <label for="security"></label>
                            <input type="text"
                            name="security"
                            id="security"
                            placeholder={{now_lang.index.Smscode}}
                            [(ngModel)]="logindata.security.value"
                            (focus)="focus('security')"security
                            />
                            <span class="secuimg" style='text-align:center'>uyes</span>
                        </div>
                        <div class="btngroup flex-r">
                            <div class="btn_loding" (click)="onsubmit()">
                                <span>{{now_lang.index.Login}}</span>
                            </div>
                            <a href="#/register" class="btn_reg">
                                <span>{{now_lang.index.Regist}}</span>
                            </a>
                        </div>
                        <div class="flex-r topnav">
                            <span>{{now_lang.Top_nav.Agent_login}}</span>|
                            <span>{{now_lang.Top_nav.Phone_stake}}</span>
                            <language></language>
                        </div>
                    </div>
                </div>
                <div class="head_mid">
                    <div class="flex-r topnav head_mid_right fl-r">
                        <span *ngFor="let item of navdatal; let i=index"  class='top_nav_item' [ngClass]="{'navhover': item.isover}"
                        (mouseenter)="navmouseenter(i,'left')" (mouseleave)="navmouseleave(i,'left')" (click)="linkrouter(item.link)">{{item.text}}</span>
                        <span *ngFor="let item of navdatar; let i=index"  class='top_nav_item' [ngClass]="{'navhover': item.isover}"
                        (mouseenter)="navmouseenter(i,'right')" (mouseleave)="navmouseleave(i,'right')">{{item.text}}</span>
                    </div>
                </div>

            </div>
            <div class="top_banner">
                <div class="lunBo" id="LunBo">

                    <div class="other"id="Other">


                        <div class="point">

                            <span class="dot_chan red 1" id='1'></span>

                            <span class='dot_chan 2' id='2'></span>

                            <span class='dot_chan 3' id='3'></span>

                            <span class='dot_chan 4' id='4'></span>

                            <span class='dot_chan 5' id='5'></span>

                        </div>

                    </div>

                    <ul style="height:100%;">

                        <li class="active"><a href="javascript:"><img src="../images/head_top/banner.png" alt="1"></a></li>

                        <li><a href="javascript:"><img src="../images/head_top/banner.png" alt="2"></a></li>

                        <li><a href="javascript:"><img src="../images/head_top/banner.png"  alt="3"></a></li>

                        <li><a href="javascript:"><img src="../images/head_top/banner.png" alt="4"></a></li>

                        <li><a href="javascript:"><img src="../images/head_top/banner.png" alt="5"></a></li>

                    </ul>

                </div>
                <div class="banner_mid_box">
                    <div class="tab_smabox">
                        <div class="hot-card paihang-card">
                            <div class="tab_btn">
                                <span [ngClass]="{'tableactive': tableactive===0}" (click)="tableactive=0">{{now_lang.index.Bet_rank}}
                                    <span class='bot_line' *ngIf="tableactive===0">
                                        
                                    </span>
                                </span>
                                <span [ngClass]="{'tableactive': tableactive===1}" (click)="tableactive=1">{{now_lang.index.Win_rank}}
                                    <span class='bot_line' *ngIf="tableactive===1">
                                        
                                    </span>
                                </span>
                            </div>
                            <div class="flex-c tab_info" *ngIf="tableactive===0">
                                <div class="flex-w info_tr" *ngFor="let item of touzhu_list;let i = index;">
                                    <div class="rank_num">
                                        <div [class]="'num'+i">
                                            {{i+1}}
                                        </div>
                                    </div>
                                    <div class="flex-w info-text">
                                        <span>QWE******</span>
                                        <span>
                                            <span style="color:#b61010;">12345678</span>元</span>
                                    </div>
                                </div>

                            </div>

                            <div class="flex-c tab_info" *ngIf="tableactive===1">
                                <div class="flex-w info_tr" *ngFor="let item of touzhu_list;let i = index;">
                                    <div class="rank_num ">
                                        <div [class]="'num'+i">
                                            {{i+1}}
                                        </div>
                                    </div>
                                    <div class="flex-w info-text">
                                        <span>CZH+++++</span>
                                        <span>
                                            <span style="color:#b61010;">123456</span>元</span>
                                    </div>
                                </div>

                            </div>

                        </div>
                        <div class="tab_board">
                            <div class="flex-r board_top">
                                <div class="left_go" (click)='pre_board()'>
                                    
                                </div>
                                <div class="mid_bar">
                                    
                                </div>
                                <div class="right_go" (click)='next_board()' >
                                    
                                </div>
                            </div>
                            <div class="board_bot"  >
                                {{now_board.content}}
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <div class="mid_contain">
            <div class="hot_box">
                <div class="lot_hot" *ngFor="let item of ball_list; let i=index"  (click)="linkrouter(item.link)">
                    <img class='img' [src]="item.imgsrc" alt="">
                    <div class="bot_til">
                        <div class="til_cont">
                            <div class="lot_ico">
                                <img [src]="item.icosrc" alt="">
                            </div>
                            <div class="lot_name">
                                {{item.name}}
                            </div>
                        </div>
                        <img [src]="item.bgsrc" alt="">
                    </div>
                </div>
            </div>
        </div>
        <div class="footer ">
            <div class="foot_box flex-r" >
                <div class="foot_c">
                    <div class="foot_c_cont">
                        <div class="top">
                            银行卡类型
                        </div>
                        <div class="bot bank_c">

                        </div>
                    </div>
                </div>
                <div class="foot_c">
                    <div class="foot_c_cont">
                        <div class="top">
                            新手帮助
                        </div>
                        <div class="bot  ">
                            <a href="">〉存款帮助</a>
                            <a href="">〉取款帮助</a>
                            <a href="">〉联系我们</a>
                            <a href="">〉常见问题</a>
                        </div>
                    </div>
                </div>
                <div class="foot_c">
                    <div class="foot_c_cont">
                        <div class="erweima">
                            <img src="../images/head_top/erweima.png"  alt="">
                        </div>
                        <div class="app_til">
                            手机APP
                        </div>
                        <a class="app_bot" href="">

                            手机APP下载
                        </a>
                    </div>
                </div>
            </div>

        </div>
</div>








