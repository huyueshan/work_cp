*{
    box-sizing: border-box;
}
.flex-r {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-content: center;
    align-items: center;
}

.flex-c {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-content: space-between;
}

.bg_1 {
    background-color: #f5f5f5;
}

.mycontainer {
    /* width: 950px; */
    padding: 0;
    margin-top: 10px;
    align-content: flex-start;
    align-items: flex-start;
    color: #333;
    width: 98%;
    margin: 10px 1%; 
}

.itemr_box {
    border-bottom: 1px solid #E6E6E6;
    /* padding: 0 10px; */
}

.itemr_box:last-child {
    border-bottom: none;
}

.querycontent {
    width: 100%;
    border-bottom: 1px solid #E6E6E6;
    /* background-color: #f5f5f5; */

}
.querycontent1{
    width: 100%;
    height: 50px;
    padding: 0 10px;    
}

.querycontent1 {
    width: 100%;
    height: 50px;
    padding: 0 10px;
}

.query_item {
    flex: none;
    padding-right: 12px;
    .left_label{
        width: 100px;
        line-height: 50px;
        padding-left: 20px;
        display: inline-block;
        min-height: 50px;
        color: #676767;
        background-color: #F5F5F5;
        flex-shrink: 0;
        align-self: stretch;
        vertical-align: middle;
        font-size: 14px;
        margin-right: 20px;
    }

}

input {
    width: 132px;
    height: 30px;
    line-height: 20px;
    padding: 5px 0 4px 5px;
    border: 1px solid #ccc;

}

input[type="checkbox"] {
    width: 16px;
    height: 16px;
    border: 1px solid #ccc;
    background-color: #fff;
    margin-top: 0;
}

.checkitems {
    margin-right: 5px;
}

label {
    font-size: 14px;
    font-weight: normal;
}

.datebtn {
    display: inline-block;
    width: 60px;
    height: 30px;
    color: white;

    line-height: 28px;
    text-align: center;
    background-color: white;
    border: 1px solid #C6C6C6;
    background-color: #C6C6C6;
    /* border: 1px solid #C6C6C6; */
    border-radius: 15px;
    cursor: pointer;
}

.bd_y,
.datebtn:hover {
    border: 1px solid transparent;
    background-color: #FA5D3E;

}

.querybtn,
.querybtn2:hover {

    width: 70px;
    height: 30px;
    display: inline-block;
    line-height: 28px;
    text-align: center;
    color: white;
    background: -webkit-linear-gradient(#D63C3E, #B52036); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#D63C3E, #B52036); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#D63C3E, #B52036); /* Firefox 3.6 - 15 */
    background: linear-gradient(#D63C3E, #B52036); /* 标准的语法 */
    border: 1px solid transparent;
    border-radius: 4px;
    cursor: pointer;
}

.querybtn2 {
    margin-left: 10px;
    background-color: #ccc;
    color: white;
    border: 1px solid #999;
}
.querybtn_big{
    width: 120px;
    height: 50px;
    display: inline-block;
    line-height: 50px;
    font-size: 16px;
    text-align: center;
    color: white;
    background: -webkit-linear-gradient(#ff6161, #e24545); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#ff6161, #e24545); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#ff6161, #e24545); /* Firefox 3.6 - 15 */
    background: linear-gradient(#ff6161, #e24545); /* 标准的语法 */
    border: 1px solid #b52525;
    border-radius: 8px;
    cursor: pointer;
}

.querybtn_big:hover{
    background: -webkit-linear-gradient(#ff6767, #ed6b6b); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#ff6767, #ed6b6b); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#ff6767, #ed6b6b); /* Firefox 3.6 - 15 */
    background: linear-gradient(#ff6767, #ed6b6b); /* 标准的语法 */
    border: 1px solid #b52525;
}
.checkitems {
    outline: none;
    width: 14px;
    height: 14px;
}

.checkboxs {
    width: 100%;
    padding: 10px;
}

.checkcontent {
    width: 100%;
    border: 1px solid #ccc;
    font-size: 12px;
    line-height: 26px;
}

.checkwrap {
    padding: 0 6px;
}

.checkwrapitem {
    width: 200px;
    height: 28px;
    line-height: 28px;
    justify-content: flex-start;
}

.checktitle {

        width: 100px;
        min-height: 50px;
        color: #676767;
        background-color: #F5F5F5;
        flex-shrink: 0;
        align-self: stretch;
        vertical-align: middle;
        font-size: 14px;
        display: flex;
        justify-content:center;
        align-items:Center;

}

.tablebox {
    width: 100%;
    padding: 10px 0 0;
}

.tablewrap {
    width: 100%;
    overflow-x: scroll;
    border: 1px solid #ccc;
}

#table {
    table-layout: fixed;
    border: 1px solid #ccc;
    border-collapse: collapse;
    width: 1400px;
}

#table th {
    font-size: 14px;
    font-weight: bold;
}

#table th,
#table td {
    text-align: center;
    height: 40px;
    /* border: 1px solid #ccc; */
}

.weight td {
    font-weight: bold;
}

.color_y {
    color: #989898;
}
.query_item #starttime,.query_item #endtime,.query_item #username{
    min-width: 120px;
    height: 28px;
    margin-top: 3px;
}
.page {
    align-self: center;
}
.btn_box{
    display: block;
    height: 100px;
    width: 100%;
    line-height: 100px;
    padding-left: 150px;
}
.checksbox{
    width: 100%;
}
.checksbox_r{

    padding: 10px 0;
    padding-left: 20px;
}

.container_2{
    .tablebox {
        width: 100%;
        padding: 10px 0 0;
    }

    .tablewrap {
        width: 98%;
        margin: 0 auto;
        overflow-x: hidden;
        
        border: 1px solid #F5F5F5;
        border-top: 2px solid #636363;
    }
    #get_more{
        height: 40px;
        width: 100%;
        font-size: 16px;
        text-align: center;
    }
    #get_more td{
        background: url("../../../images/user_center/more.png") center left 47% no-repeat ;
        text-align: center!important;
    }
    #table {
        table-layout: fixed;
        /* border: 1px solid #ccc; */
        border-collapse: collapse;
        width: 100%;
        border: none;
    }

    #table th,
        {
        font-size: 14px;
        font-weight: bold;
        text-align: left;
        color: #666666;
    }

    #table th,
    #table td {
        text-align: left;
        text-indent: 15px;
        height: 40px;
        /* border: 1px solid #ccc; */
    }
    #table td{
       color: #333333 
    }
    #table tr:nth-child(odd){
       background-color: #F5F5F5;
    }
    #table tr:nth-last-child(1){
        cursor: pointer;
        background-color: #F5F5F5;
    }
    .page {
        align-self: center;
    }
}