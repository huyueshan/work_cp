// 各浏览器滑块样式
input[type="range"] {
    display: block;
    position: relative;
    width: 100%;
    height: 30px;
    background: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    cursor: pointer;
    outline: none;
}
input[type="range"]:focus{
    outline:none;
}

// range中心圆点
input[type="range"]::-webkit-slider-thumb {
    -webkit-appearance: none;
    appearance: none;
    height: 12px;
    width: 18px;
    border: none;
    border-radius: 5px;
    background-color: #b61010;
    cursor: pointer;
    margin-top: -4px;
}

input[type="range"]::-moz-range-thumb {
    -moz-appearance: none;
    appearance: none;
    height: 12px;
    width: 18px;
    border: none;
    border-radius: 5px;
    background-color: #b61010;
    cursor: pointer;
}

input[type="range"]::-ms-thumb {
    -ms-appearance: none;
    appearance: none;
    height: 12px;
    width: 18px;
    border: none;
    border-radius: 5px;
    background-color: #b61010;
    cursor: pointer;
    margin-top: 1px;
}

input[type="range"]::slider-thumb {
    -webkit-appearance: none;
    appearance: none;
    height: 12px;
    width: 18px;
    border: none;
    border-radius: 5px;
    background-color: #b61010;
    cursor: pointer;
    margin-top: -6px;
}

// range轨道
input[type="range"]::-webkit-slider-runnable-track {
    width: 100%;
    height: 4px;
    background-color: #b61010;;
    border-radius: 2px;
    cursor: pointer;
}

input[type="range"]::-moz-range-track {
    width: 100%;
    height: 4px;
    background-color: #b61010;;
    border-radius: 2px;
    cursor: pointer;
}

input[type="range"]::-ms-track {
    width: 100%;
    height: 2px;
    border-color: transparent;
    border-radius: 2px;
    background: #b61010;
    cursor: pointer;
}
