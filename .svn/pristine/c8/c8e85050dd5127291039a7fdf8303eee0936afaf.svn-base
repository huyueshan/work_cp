.pccontain {
    width: 100%;
    align-self: center;
    background-size: cover;
    .tabbox {
        width: 100%;
        padding-right: 20px;
        justify-content: space-between;
        background-color: #3B3B3B;
        .tabcontent {
            span {
                position: relative;
                height: 36px;
                line-height: 36px;
                padding: 0 18px;
                color: white;
                cursor: pointer;
                z-index: 9;
            }
            .tabactive {
                background-color: #b61010;
            }
            .tabactive:before {
                content: '';
                position: absolute;
                top: 36px;
                left: 50%;
                margin-left: -9px;
                width: 0;
                height: 0;
                border-left: 8px solid transparent;
                border-top: 8px solid #B61010;
                border-right: 8px solid transparent;
            }
        }
        .togglebtn {
            height: 23px;
            line-height: 21px;
            border-radius: 11px;
            background-color: #575757;;
            overflow: hidden;
            span {
                width: 70px;
                height: 100%;
                line-height: 21px;
                text-align: center;
                color:#fff;
                border-radius: 11px;
                background-color: #575757;
                cursor: pointer;
            }
            .togactive{
                color: #fff;
                background-color: #B61010;
            }
        }
    }
    .contaner {
        width: 1000px;
        align-self: center;
        .mainbox {
            position: relative;
            width: 100%;
            justify-content: space-between;
            align-content: flex-start;
            align-items: flex-start;
            .contentbox {
                width: 100%;
                align-self: stretch;
                .selctbox {
                    width: 100%;
                    height: 60px;
                    padding: 0 20px 0 0;
                    justify-content: space-between;
                    background-color: white;
                    border-bottom: 2px solid #666;
                    .rangebox {
                        .showvalue {
                            flex: 0 0 70px;
                            width: 50px;
                            text-align: right;
                            padding-right: 20px;
                            color: #666;
                        }
                        .icon {
                            flex: none;
                            width: 20px;
                            height: 20px;
                            font-size: 14px;
                            line-height: 16px;
                            text-align: center;
                            font-weight: bold;
                            border: 1px solid #999;
                            border-radius: 2px;
                            background-color: white;
                            color: #999;
                            cursor: pointer;
                        }
                        .range {
                            width: 130px;
                            padding: 0 10px;
                            align-items: center;
                        }
                    }
                    .btngroup {
                        button {
                            width: 76px;
                            font-size: 12px;
                            height: 26px;
                            line-height: 24px;
                            text-align: center;
                            padding: 0;
                            color: #333;
                            background: linear-gradient(to top, #E3E3E3, #F9F9F9);
                            border: 1px solid #999;
                            border-radius: 2px;
                            margin-left: 8px;
                            outline: none;
                            cursor: pointer;
                        }
                        .lactive {
                            margin-left: 0px;
                        }
                        .btnactive,
                        .resetbtn:hover,
                        button:hover {
                            position: relative;
                            color: #b61010;
                            background-color: white;
                            border: 1px solid #b61010;
                            outline: none;
                        }
                        .btnactive:before {
                            content: '';
                            position: absolute;
                            right: 0;
                            bottom: 0;
                            width: 15px;
                            height: 14px;
                            background: url('../../images/mod2/icon_01.png') -10px -76px;
                        }
                    }
                }
            }
        }
    }
}
