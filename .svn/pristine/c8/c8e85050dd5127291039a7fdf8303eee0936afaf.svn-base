        transform: scale(.6);
        transition: transform .1s linear;
        .top {
            width: 100%;
            height: 40px;
            line-height: 40px;
            font-size: 14px;
            color: white;
            padding: 0 12px 0 20px;
            justify-content: space-between;
            background: linear-gradient(to top, #cf364c, #ec5c5c);
            i {
                font-size: 18px;
                cursor: pointer;
            }
        }
        .main {
            padding: 5px 12px 20px 20px;
            p {
                font-size: 10px;
                line-height: 20px;
                color: #999;
            }
            .setinput {
                width: 100%;
                justify-content: center;
                .item {
                    width: 100%;
                    font-size: 12px;
                    padding: 5px 0;
                    justify-content: center;
                    input {
                        width: 100px;
                        height: 30px;
                        line-height: 28px;
                        padding-left: 5px;
                        border: 1px solid #ccc;
                    }
                    span {
                        width: 50px;
                        height: 30px;
                        line-height: 30px;
                        text-align: center;
                        letter-spacing: 4px;
                        padding-left: 4px;
                        border-radius: 2px;
                        margin-left: 10px;
                        color: white;
                        background-color: #999;
                        cursor: pointer;
                    }
                }
            }
            .btnbox {
                width: 100%;
                justify-content: center;
                padding: 10px 0;
                span {
                    width: 75px;
                    height: 30px;
                    line-height: 30px;
                    text-align: center;
                    font-size: 14px;
                    background-color: #999;
                    color: white;
