<div class="flex-c" style="min-width:1360px;" [hidden]="!loadpage">
    <div>
        <app-header [username]="'test08'" [curpath]="'lottery'"></app-header>
    </div>
    <div class="flex-c guan" *ngIf="curgamestyle === '官'">
        <div class="flex-r result-head">
            <div class="flex-r result-head-in">
                <div class="game">
                    <label></label>
                    <select class="gamelist" [(ngModel)]="quergame" (change)="selechange()">
                        <option *ngFor="let item of gamelist; let i=index">{{item.gamename}}</option>
                    </select>
                </div>
                <div class="flex-r checkbox-box">
                    标注形式选择
                    <label class="flex-r">
                        <input type="checkbox" name="checkbox" id="freeLine" [(ngModel)]="view_paramet.isshowline" />显示走势折线
                    </label>
                    <label class="flex-r">
                        <input type="checkbox" name="checkbox" value="checkbox" id="haowen" [(ngModel)]="view_paramet.ishaowen" />显示号温
                    </label>
                    <label class="flex-r">
                        <input type="checkbox" name="checkbox" value="checkbox" id="yl_zhu" [(ngModel)]="view_paramet.isomission" />显示遗漏分层
                    </label>
                    <label class="flex-r">
                        <input type="checkbox" name="checkbox2" value="checkbox" id="c_yl" [(ngModel)]="view_paramet.isomissionshow" />显示遗漏
                    </label>
                    <label class="flex-r">
                        <input type="checkbox" name="checkbox3" value="checkbox" id="disdata" [(ngModel)]="view_paramet.isdisdata" />显示统计
                    </label>
                    <label class="flex-r">
                        <input type="checkbox" name="checkbox3" value="checkbox" id="help_line" [(ngModel)]="view_paramet.issubline" />显示辅助线
                    </label>
                </div>
                <div class="flex-r period">
                    <a *ngFor="let item of numbdata; let i=index" [ngClass]="{active: numbdatactve===i}" (click)="numbclick(i)">最近{{item}}期</a>
                </div>
            </div>
        </div>
        <div class="loadinginfo" *ngIf="loadinfo">
            <p>正在获取数据中...</p>
        </div>
        <div class="flex-c main">
            <div class="flex-r mainbox" *ngIf="!loadinfo">
                <!-- 官方表格划线彩种 -->
                <div class="flex-r result-main" *ngIf="curgametype==='ssc'||curgametype==='exf'||curgametype==='pk10'||curgametype==='fc3d'||curgametype==='k3'">
                    <app-canvas [viewdata]="view_paramet" [resultdata]="resultdata" [omdata]="resultomdata" [type]="curgametype" #canvas></app-canvas>
                </div>

                <!-- 官方快乐8系列 -->
                <div class="flex-r result-main" *ngIf="curgametype==='kl8'">
                    <div class="flex-c tablebox">
                        <table width=auto border="0" cellpadding="0" cellspacing="0" align="center">
                            <thead>
                                <tr>
                                    <th style="width: 100px">期号</th>
                                    <th colspan="20">开奖号码</th>
                                    <th>大小</th>
                                    <th>单双</th>
                                    <th>上下</th>
                                    <th>奇偶</th>
                                    <th>总计</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr *ngFor="let item of resultdata">
                                    <td>{{item.number}}</td>
                                    <td *ngFor="let child of item.data">{{child}}</td>
                                    <td class="color1">{{item.bigSmall}}</td>
                                    <td>{{item.danShuang}}</td>
                                    <td class="color1">{{item.upDown}}</td>
                                    <td>{{item.evenOdd}}</td>
                                    <td class="color1">{{item.sum}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="flex-c xin" *ngIf="curgamestyle === '信'">
        <div class="flex-r result-head">
            <div class="flex-r result-head-in">
                <div class="game">
                    <label>游戏：</label>
                    <select class="gamelist" [(ngModel)]="quergame" (change)="selechange()">
                        <option *ngFor="let item of gamelist; let i=index">{{item.gamename}}</option>
                    </select>
                </div>
                <div class="flex-r querybox">
                    <label class="flex-r">范围：
                        <input type="date" id="starttime" name="starttime" [(ngModel)]="querydata.starttime" />
                     ~
                        <input type="date" id="endtime" name="endtime" [(ngModel)]="querydata.endtime" />
                    </label>
                    <label class="flex-r">期数：
                        <input type="text" id="qishu" name="qishu" [(ngModel)]="querydata.qishu" />
                    </label>
                    <span id="query" (click)="query()">查询</span>
                </div>
                <div class="flex-r period">
                    <span>搅珠日期表</span>
                    <span class="reset">刷新</span>
                    <select name="rest" id="serest" [(ngModel)]="querydata.resttime">
                        <option value="0" selected>不刷新</option>
                        <option value="1">10</option>
                        <option value="2">30</option>
                        <option value="3">60</option>
                        <option value="4">120</option>
                        <option value="5">500</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="loadinginfo" *ngIf="loadinfo">
                <p>正在获取数据中...</p>
        </div>
        <!-- 11选5 -->
        <ng-container *ngIf="curgametype==='exf'" >
            <div class="flex-r tab" *ngIf="!loadinfo">
                <span *ngFor="let item of xintab.list; let i=index"
                [ngClass]="{active: xintab.active === i}" 
                (click)="xintab.active = i" >{{item}}</span>
            </div>
            <div class="flex-c main">
                <div class="flex-r mainbox" *ngIf="!loadinfo">
                    <div class="flex-r result-main">
                            <div class="flex-c tablebox">
                                <ng-container *ngIf="xintab.active === 0" >
                                    <table width=auto border="0" cellpadding="0" cellspacing="0" align="center">
                                        <thead>
                                            <tr>
                                                <th rowspan="2" style="width: 100px">期数</th>
                                                <th rowspan="2" style="width: 180px">开奖时间</th>
                                                <th colspan="5">彩球号码</th>
                                                <th rowspan="2">总和</th>
                                                <th rowspan="2">总单双</th>
                                                <th rowspan="2">总大小</th>
                                                <th rowspan="2" style="width: 80px">总尾大小</th>
                                                <th colspan="2">正码一</th>
                                                <th colspan="2">正码二</th>
                                                <th colspan="2">正码三</th>
                                                <th colspan="2">正码四</th>
                                                <th colspan="2">特码</th>
                                                <th colspan="2">上下奇偶</th>
                                            </tr>
                                            <tr class="childtr">
                                                <th>正码一</th>
                                                <th>正码二</th>
                                                <th>正码三</th>
                                                <th>正码四</th>
                                                <th>特码</th>
                                                <th>大小</th>
                                                <th>单双</th>
                                                <th>大小</th>
                                                <th>单双</th>
                                                <th>大小</th>
                                                <th>单双</th>
                                                <th>大小</th>
                                                <th>单双</th>
                                                <th>大小</th>
                                                <th>单双</th>
                                                <th>上下</th>
                                                <th>奇偶</th>
        
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr *ngFor="let item of resultdata">
                                                <td class="nomal">{{item.number}}</td>
                                                <td class="nomal">{{item.time}}</td>
                                                <td *ngFor="let child of item.data.balls"> <i>{{child}}</i> </td>
                                                <td class="nomal">{{item.data.sum}}</td>
                                                <td [ngClass]="{color1:item.data.sumOddEven==='总单',nomal:item.data.sumOddEven==='和'}" >{{item.data.sumOddEven}}</td>
                                                <td [ngClass]="{color1:item.data.sumBigSmall==='总小',nomal:item.data.sumBigSmall==='和'}" >{{item.data.sumBigSmall}}</td>
                                                <td [ngClass]="{color1:item.data.lastBigSmall==='总尾小'}">{{item.data.lastBigSmall}}</td>
                                                <ng-container *ngFor="let child of item.data.balls">
                                                    <td [ngClass]="{color1:item.data.OneBigSmall(child)==='小',nomal:item.data.OneBigSmall(child)==='和'}">{{item.data.OneBigSmall(child) }}</td>
                                                    <td [ngClass]="{color1:item.data.OneOddEven(child)==='单',nomal:item.data.OneOddEven(child)==='和'}">{{item.data.OneOddEven(child) }}</td>
                                                </ng-container>
                                                <td [ngClass]="{color1:item.data.upDown==='上',nomal:item.data.upDown==='和'}">{{item.data.upDown}}</td>
                                                <td [ngClass]="{color1:item.data.oddEven==='奇',nomal:item.data.oddEven==='和'}">{{item.data.oddEven}}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </ng-container>
                                <ng-container *ngIf="xintab.active === 1" >
                                    <table width=auto border="0" cellpadding="0" cellspacing="0" align="center">
                                        <thead>
                                            <tr>
                                                <th rowspan="2" style="width: 100px">期数</th>
                                                <th rowspan="2" style="width: 180px">开奖时间</th>
                                                <th colspan="5">彩球号码</th>
                                                <th colspan="10">龙虎斗</th>
                                            </tr>
                                            <tr class="childtr">
                                                <th>正码一</th>
                                                <th>正码二</th>
                                                <th>正码三</th>
                                                <th>正码四</th>
                                                <th>正码五</th>
                                                <th *ngFor="let item of longhulist.exf" >{{item}}</th>  
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr *ngFor="let item of resultdata">
                                                <td class="nomal">{{item.number}}</td>
                                                <td class="nomal">{{item.time}}</td>
                                                <td *ngFor="let child of item.data.balls">
                                                    <i [ngClass]="{bg_red:color(child)==='red',bg_blue:color(child)==='blue',bg_green:color(child)==='green'}">{{child}}</i>
                                                </td>
                                                <td [ngClass]="{color1:item.data.pklh(1,2)==='龙',nomal:item.data.pklh(1,2)==='和'}">{{item.data.pklh(1,2)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(1,3)==='龙',nomal:item.data.pklh(1,3)==='和'}">{{item.data.pklh(1,3)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(1,4)==='龙',nomal:item.data.pklh(1,4)==='和'}">{{item.data.pklh(1,4)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(1,5)==='龙',nomal:item.data.pklh(1,5)==='和'}">{{item.data.pklh(1,5)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(2,3)==='龙',nomal:item.data.pklh(2,3)==='和'}">{{item.data.pklh(2,3)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(2,4)==='龙',nomal:item.data.pklh(2,4)==='和'}">{{item.data.pklh(2,4)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(2,5)==='龙',nomal:item.data.pklh(2,5)==='和'}">{{item.data.pklh(2,5)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(3,4)==='龙',nomal:item.data.pklh(3,4)==='和'}">{{item.data.pklh(3,4)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(3,5)==='龙',nomal:item.data.pklh(3,5)==='和'}">{{item.data.pklh(3,5)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(4,5)==='龙',nomal:item.data.pklh(4,5)==='和'}">{{item.data.pklh(4,5)}}</td> 
                                            </tr>
                                        </tbody>
                                    </table>
                                </ng-container>
                            </div>
                    </div>
                </div>
            </div>
        </ng-container>
        <!-- 时时彩 -->
