                                                <th colspan="7">彩球号码</th>
                                                <th colspan="2">上下奇偶</th>
                                                <th colspan="21">龙虎斗</th>
                                            </tr>
                                            <tr class="childtr">
                                                <th style="width: 100px">正码一</th>
                                                <th style="width: 100px">正码二</th>
                                                <th style="width: 100px">正码三</th>
                                                <th style="width: 100px">正码四</th>
                                                <th style="width: 100px">正码五</th>
                                                <th style="width: 100px">正码六</th>
                                                <th style="width: 100px">特码</th>
                                                <th>上下</th>
                                                <th>奇偶</th>
                                                <th *ngFor="let item of longhulist.lhc" >{{item}}</th>  
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr *ngFor="let item of resultdata">
                                                <td class="nomal">{{item.number}}</td>
                                                <td class="nomal">{{item.time}}</td>
                                                <td class="nomal" *ngFor="let child of item.data.balls">
                                                    <i [ngClass]="{bg_red:color(child.num)==='red',bg_blue:color(child.num)==='blue',bg_green:color(child.num)==='green'}">{{child.num}}</i>{{child.name}}
                                                </td>
                                                <td [ngClass]="{color1:item.data.upDown==='上',nomal:item.data.upDown==='和'}">{{item.data.upDown}}</td>
                                                <td [ngClass]="{color1:item.data.oddEven==='奇',nomal:item.data.oddEven==='和'}">{{item.data.oddEven}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(1,2)==='龙',nomal:item.data.pklh(1,2)==='和'}">{{item.data.pklh(1,2)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(1,3)==='龙',nomal:item.data.pklh(1,3)==='和'}">{{item.data.pklh(1,3)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(1,4)==='龙',nomal:item.data.pklh(1,4)==='和'}">{{item.data.pklh(1,4)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(1,5)==='龙',nomal:item.data.pklh(1,5)==='和'}">{{item.data.pklh(1,5)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(1,6)==='龙',nomal:item.data.pklh(1,6)==='和'}">{{item.data.pklh(1,6)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(1,7)==='龙',nomal:item.data.pklh(1,7)==='和'}">{{item.data.pklh(1,7)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(2,3)==='龙',nomal:item.data.pklh(2,3)==='和'}">{{item.data.pklh(2,3)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(2,4)==='龙',nomal:item.data.pklh(2,4)==='和'}">{{item.data.pklh(2,4)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(2,5)==='龙',nomal:item.data.pklh(2,5)==='和'}">{{item.data.pklh(2,5)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(2,6)==='龙',nomal:item.data.pklh(2,6)==='和'}">{{item.data.pklh(2,6)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(2,7)==='龙',nomal:item.data.pklh(2,7)==='和'}">{{item.data.pklh(2,7)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(3,4)==='龙',nomal:item.data.pklh(3,4)==='和'}">{{item.data.pklh(3,4)}}</td>
                                                <td [ngClass]="{color1:item.data.pklh(3,5)==='龙',nomal:item.data.pklh(3,5)==='和'}">{{item.data.pklh(3,5)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(3,6)==='龙',nomal:item.data.pklh(3,6)==='和'}">{{item.data.pklh(3,6)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(3,7)==='龙',nomal:item.data.pklh(3,7)==='和'}">{{item.data.pklh(3,7)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(4,5)==='龙',nomal:item.data.pklh(4,5)==='和'}">{{item.data.pklh(4,5)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(4,6)==='龙',nomal:item.data.pklh(4,6)==='和'}">{{item.data.pklh(4,6)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(4,7)==='龙',nomal:item.data.pklh(4,7)==='和'}">{{item.data.pklh(4,7)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(5,6)==='龙',nomal:item.data.pklh(5,6)==='和'}">{{item.data.pklh(5,6)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(5,7)==='龙',nomal:item.data.pklh(5,7)==='和'}">{{item.data.pklh(5,7)}}</td> 
                                                <td [ngClass]="{color1:item.data.pklh(6,7)==='龙',nomal:item.data.pklh(6,7)==='和'}">{{item.data.pklh(6,7)}}</td> 
                                            </tr>
                                        </tbody>
                                    </table>
                                </ng-container>
                            </div>
                    </div>
                </div>
            </div>
        </ng-container>

    </div>

</div>