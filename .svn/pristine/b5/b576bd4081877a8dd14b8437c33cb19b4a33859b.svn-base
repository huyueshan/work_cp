                                            <input type="text" maxlength="9" class="inmoney" placeholder="下注金额" name="allinmoney" [(ngModel)]="kuaitoudata.value" value=""
                                                (focus)="inmoneyfocus($event,'kuaitou')" (blur)="inmoneyblur()" (change)="allchange()"
                                                (keyup)="changereg()">
                                        </span>
                                        <span class="btnactive" (click)="ktclick('rest', 0)">清空选号</span>
