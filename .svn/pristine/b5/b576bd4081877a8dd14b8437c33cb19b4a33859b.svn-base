                                            <ul class="flex-r infobox">
                                                <li class="flex-r" *ngFor="let item of kuaitoudata.data3;let i=index" [ngClass]="{active:item.checked}" (click)="ktclick('data3',i)">{{item.name}}</li>
                                            </ul>
