                                <div class="flex-r top">
                                    <span [ngClass]="{active: kuaitoutab === 0}" (click)="kuaitoutab=0"> 快速投注 </span>
                                    <span [ngClass]="{active: kuaitoutab === 1}" (click)="kuaitoutab=1">键盘下注</span>
                                </div>
                                <div class="flex-r main" *ngIf="kuaitoutab===0" >
                                    <div class="flex-r optionbox optionbox1">
                                        <ul class="flex-r infobox">
                                            <li class="flex-r" *ngFor="let item of kuaitoudata.data1;let i=index" [ngClass]="{active:item.checked}" (click)="ktclick('data1',i)">{{item.numb}}</li>
                                        </ul>

                                    </div>
                                    <div class="flex-r optionbox optionbox2">
                                        <div class="flex-r optionbox optionbox2">
                                            <ul class="flex-r infobox">
                                                <li class="flex-r" *ngFor="let item of kuaitoudata.data2;let i=index" [ngClass]="{active:item.checked}" (click)="ktclick('data2',i)">{{item.name}}</li>
                                            </ul>
                                        </div>
