                            <div class="flex-r conmain">
                                <div class="flex-r conmainbox conmainbox2">
                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">生肖</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3">勾选</span>
                                        </span>
                                        <span class="flex-r item4box">号码</span>
                                    </div>
                                </div>
                                <div class="flex-r conmainbox conmainbox2">

                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">生肖</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3">勾选</span>
                                        </span>
                                        <span class="flex-r item4box">号码</span>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-r conmain">
                                <div class="flex-r conmainbox conmainbox2" *ngFor="let item of dpcdata12.data1; let i=index">
                                    <span class="flex-r itembox">
                                        <span class="flex-r item1 " *ngIf="item.name !== null">
                                            {{item.name}}
                                        </span>
                                        <span class="item2 color_y" *ngIf="item.name !== null">{{(item.point+(item.step*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3">
                                            <input type="checkbox" name="check" [(ngModel)]="item.checked" [disabled]="item.disabled" (change)="chechange(i,1)" />
                                        </span>
                                    </span>
                                    <span class="flex-r item4box">
                                        <i *ngFor="let child of item.data" [ngClass]="{bgred: child.red , bgblue: child.blue, bggreen: child.green }">{{child.numb}}</i>
                                    </span>
                                </div>
                            </div>
                            <div class="flex-r electbox">
                                <span class="flex-r btngroup">
                                    <button *ngFor="let item of optionsdata.lianxiaostyle; let i=index" [ngClass]="{active: optionsdata.lianxiaostactive === i}"
                                        (click)="lianxiaoclick(i,0)">{{item}}</button>
                                </span>

                            </div>
                        </div>

                        <!-- // 龙虎斗 -->
                        <div class="flex-c content" *ngIf="type === 13">
                            <div class="flex-r conmain2">
                                <div class="itemwrap2" *ngFor="let item of dpcdata13;let i=index">
                                    <div class="flex-c itembox2">
                                        <span class="item1">
                                            {{item.title}}
                                        </span>
