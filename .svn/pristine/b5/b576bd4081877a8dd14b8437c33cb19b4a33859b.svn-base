<div class="flex-c sscbox" [hidden]="!loadpage">
    <div class="flex-c pccontain">
        <div class="contaner">
            <app-credtop [type]="'pk10'" [indata]="cpnav"></app-credtop>
            <div class="flex-r mainbox">
                <div class="flex-c contentbox">
                    <div class="flex-r tabbox">
                        <div class="flex-r tabcontent">
                            <span *ngFor="let item of typedata" [ngClass]="{tabactive:this.type == item.id}" (click)="togtype(item.id)">{{item.name}}</span>
                        </div>
                    </div>
                    <div class="flex-r selctbox">
                        <span class="flex-r">
                            <strong>$</strong>
                            <span class="flex-r rangebox">
                                <span class="showvalue" *ngIf="type===7">0%</span>
                                <ng-container *ngIf="!(type===7)" >
                                    <span class="showvalue">{{(odds-rangevalue)| number : '1.0-1'}}%</span>

                                    <span class="icon" (click)="rangevaluelessen()">-</span>
                                    <span class="flex-r range">
                                        <input type="range" name="points" min="0" max="{{odds}}" step="{{rastep}}" id="points" [(ngModel)]="rangevalue">
                                    </span>
                                    <span class="icon" (click)="rangevalueadd()">+</span>
                                </ng-container>
                            </span>
                        </span>
                        <span class="flex-r btngroup">
                            <button [ngClass]="{btnactive:selectbtnvalue === 0}" (click)="tabclick(0)" [hidden]="type===7 ||type===10 ||type===11 ||type===12">一般</button>
                            <button [ngClass]="{btnactive:selectbtnvalue === 1}" (click)="tabclick(1)" [hidden]="type===7 ||type===10 ||type===11 ||type===12">快捷</button>
                            <button class="kuxuan" (click)="tabclick(2)">快选金额</button>
                        </span>
                    </div>
                    <div class="flex-c mycontaion">

                        <!-- 特码 -->
                        <div class="flex-c content content1" *ngIf="type === 1">
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox" *ngFor="let item of contenttoptitle;">
                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">号码</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" *ngIf="inputshow">金额</span>
                                        </span>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-c conmain" style="height: 400px;">
                                <div class="flex-c conmainbox" *ngFor="let item of dpcdata1.data1; let i=index">
                                    <span class="flex-r itembox">
                                        <span class="flex-r item1 " *ngIf="item.numb !== null">
                                            <i [ngClass]="{bgred: item.red , bgblue: item.blue, bggreen: item.green }">{{item.numb}}</i>
                                        </span>
                                        <span class="item2 color_y" *ngIf="item.numb !== null">{{(item.point+(item.step*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" *ngIf="inputshow && item.numb !== null">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="item.value" (focus)="inmoney1focus($event, 1,'data1', i,null)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()">
                                        </span>
                                    </span>
                                </div>
                            </div>
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox" *ngFor="let item of dpcdata1.data2; let i=index">
                                    <span class="flex-r itembox" *ngFor="let child of item; let q=index">
                                        <span class="item1" *ngIf="child.name!==null">
                                            {{child.name}}
                                        </span>
                                        <span class="item2 color_y" *ngIf="child.name!==null">{{(child.point+(child.step*rangevalue))| number : '1.0-3'}} </span>
                                        <span class="item3" *ngIf="inputshow">
                                            <input type="text" maxlength="9" class="inmoney" name="inmoney" [(ngModel)]="child.value" (focus)="inmoney1focus($event, 1,'data2', i, q)"
                                                (blur)="inmoneyblur()" (keyup)="changereg()" *ngIf="child.name!==null">
                                        </span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <!-- 色波 -->
                        <div class="flex-c content content1" *ngIf="type === 2">
                            <div class="flex-r conmain">
                                <div class="flex-c conmainbox conmainbox1">
                                    <div class="flex-r contenttop">
                                        <span class="flex-r itembox">
                                            <span class="item1">项目</span>
                                            <span class="item2">赔率</span>
                                            <span class="item3" *ngIf="inputshow">金额</span>
                                        </span>
                                        <span class="flex-r item4box">号码</span>
                                    </div>
                                </div>
                            </div>
                            <div class="flex-c conmain">
                                <div class="flex-r conmainbox conmainbox1" *ngFor="let item of dpcdata2.data1; let i=index">
