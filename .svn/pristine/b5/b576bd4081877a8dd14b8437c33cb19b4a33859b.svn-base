                                            <span class="item3" *ngIf="inputshow">勾选</span>
                                        </span>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-c conmain" style="height: 400px;">
                                <div class="flex-c conmainbox" *ngFor="let item of dpcdata11.data1; let i=index">
                                    <span class="flex-r itembox">
                                        <span class="flex-r item1 " *ngIf="item.numb !== null">
